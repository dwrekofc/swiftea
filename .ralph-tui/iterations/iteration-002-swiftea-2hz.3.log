n    public init(\n        id: String,\n        subject: String? = nil,\n        participantCount: Int = 0,\n        messageCount: Int = 0,\n        firstDate: Date? = nil,\n        lastDate: Date? = nil,\n        createdAt: Date? = nil,\n        updatedAt: Date? = nil\n    ) {\n        self.id = id\n        self.subject = subject\n        self.participantCount = participantCount\n        self.messageCount = messageCount\n        self.firstDate = firstDate\n        self.lastDate = lastDate\n        self.createdAt = createdAt\n        self.updatedAt = updatedAt\n    }\n}\n\n/// Protocol for thread-like types to avoid ambiguity with Foundation.Thread in CLI\npublic protocol ThreadLike {\n    var id: String { get }\n    var subject: String? { get }\n    var participantCount: Int { get }\n    var messageCount: Int { get }\n    var firstDate: Date? { get }\n    var lastDate: Date? { get }\n}\n\nextension Thread: ThreadLike {}\n","structuredPatch":[{"oldStart":649,"oldLines":7,"newStart":649,"newLines":7,"lines":["         let result = try conn.query(\"\"\"","             SELECT m.* FROM messages m","             JOIN messages_fts fts ON m.rowid = fts.rowid","-            WHERE messages_fts MATCH '\\(escapeSql(query))'","+            WHERE messages_fts MATCH '\\(escapeSql(escapeFts5Query(query)))'","             ORDER BY bm25(messages_fts)","             LIMIT \\(limit) OFFSET \\(offset)","             \"\"\")"]}],"userModified":false,"replaceAll":false}}
