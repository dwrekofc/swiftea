6â†’        XCTAssertEqual(next5.count, 5)\n  1637â†’\n  1638â†’        // Verify no overlap\n  1639â†’        let first5Ids = Set(first5.map { $0.id })\n  1640â†’        let next5Ids = Set(next5.map { $0.id })\n  1641â†’        XCTAssertTrue(first5Ids.isDisjoint(with: next5Ids))\n  1642â†’    }\n  1643â†’\n  1644â†’    func testGetThreadsForMessage() throws {\n  1645â†’        try database.initialize()\n  1646â†’\n  1647â†’        // Create threads with different dates\n  1648â†’        let baseDate = Date(timeIntervalSince1970: 1000000)\n  1649â†’        for i in 1...3 {\n  1650â†’            let thread = Thread(\n  1651â†’                id: \"msg-threads-\\(i)\",\n  1652â†’                subject: \"Thread \\(i)\",\n  1653â†’                lastDate: Date(timeIntervalSince1970: baseDate.timeIntervalSince1970 + Double(i * 100))\n  1654â†’            )\n  1655â†’            try database.upsertThread(thread)\n  1656â†’        }\n  1657â†’\n  1658â†’        let message = MailMessage(id: \"thread-query-msg\", subject: \"Query Message\")\n  1659â†’        try database.upsertMessage(message)\n  1660â†’\n  1661â†’        // Add message to all threads\n  1662â†’        for i in 1...3 {\n  1663â†’            try database.addMessageToThread(messageId: \"thread-query-msg\", threadId: \"msg-threads-\\(i)\")\n  1664â†’        }\n  1665â†’\n  1666â†’        let threads = try database.getThreadsForMessage(messageId: \"thread-query-msg\")\n  1667â†’        XCTAssertEqual(threads.count, 3)\n  1668â†’\n  1669â†’        // Verify ordering by last_date DESC\n  1670â†’        XCTAssertEqual(threads[0].id, \"msg-threads-3\")\n  1671â†’        XCTAssertEqual(threads[1].id, \"msg-threads-2\")\n  1672â†’        XCTAssertEqual(threads[2].id, \"msg-threads-1\")\n  1673â†’    }\n  1674â†’\n  1675â†’    func testGetThreadsForMessageEmpty() throws {\n  1676â†’        try database.initialize()\n  1677â†’\n  1678â†’        let threads = try database.getThreadsForMessage(messageId: \"nonexistent