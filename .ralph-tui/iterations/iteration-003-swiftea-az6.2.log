   public var isRead: Bool?\n   410â†’        public var isFlagged: Bool?\n   411â†’        public var dateAfter: Date?\n   412â†’        public var dateBefore: Date?\n   413â†’        public var hasAttachments: Bool?\n   414â†’        public var freeText: String?\n   415â†’        public var mailboxStatus: MailboxStatus?\n   416â†’\n   417â†’        public init() {}\n   418â†’\n   419â†’        /// Check if any filters are set\n   420â†’        public var hasFilters: Bool {\n   421â†’            from != nil || to != nil || subject != nil || mailbox != nil ||\n   422â†’            isRead != nil || isFlagged != nil || dateAfter != nil ||\n   423â†’            dateBefore != nil || hasAttachments != nil || mailboxStatus != nil\n   424â†’        }\n   425â†’\n   426â†’        /// Check if there's free text for FTS\n   427â†’        public var hasFreeText: Bool {\n   428â†’            freeText != nil && !freeText!.isEmpty\n   429â†’        }\n   430â†’    }\n   431â†’\n   432â†’    /// Parse a structured query string into filters\n   433â†’    /// Supports: from:, to:, subject:, mailbox:, is:read, is:unread, is:flagged, is:unflagged,\n   434â†’    /// after:, before:, date:, has:attachments\n   435â†’    public func parseQuery(_ query: String) -> SearchFilter {\n   436â†’        var filter = SearchFilter()\n   437â†’\n   438â†’        // Regex patterns for structured filters\n   439â†’        let filterPatterns: [(pattern: String, handler: (String, inout SearchFilter) -> Void)] = [\n   440â†’            // from: filter (email or name)\n   441â†’            (\"from:\\\"([^\\\"]+)\\\"\", { value, f in f.from = value }),\n   442â†’            (\"from:(\\\\S+)\", { value, f in f.from = value }),\n   443â†’\n   444â†’            // to: filter\n   445â†’            (\"to:\\\"([^\\\"]+)\\\"\", { value, f in f.to = value }),\n   446â†’            (\"to:(\\\\S+)\", { value, f in f.to = value }),\n   447â†’\n   448â†’            // subject: filter\n   449â†’            (\"subject:\\\"([^\\\"]+)\\\"\", { value, f in f.subject = value }),\n   450â†’            (\"subject:(\\\\S+)\", { value, f in f.subject = value }),\n   451â†’\n   452â†’            // mailbox: filter\n   453â†’            (\"mailbox:\\\"([^\\\"]+)\\\"\", { value, f in f.mailbox = value }),\n   454â†’            (\"mailbox:(\\\\S+)\", { value, f in f.mailbox = value }),\n   455â†’\n   456â†’            // is: status filters\n   457â†’            (\"is:read\", { _, f in f.isRead = true }),\n   458â†’            (\"is:unread\", { _, f in f.isRead = false }),\n   459â†’            (\"is:flagged\", { _, f in f.isFlagged = true }),\n   460â†’            (\"is:unflagged\", { _, f in f.isFlagged = false }),\n   461â†’\n   462â†’            // has: filters\n   463â†’            (\"has:attachments?\", { _, f in f.hasAttachments = true }),\n   464â†’\n   465â†’            // Date filters (YYYY-MM-DD format)\n   466â†’            (\"after:(\\\\d{4}-\\\\d{2}-\\\\d{2})\", { value, f in f.dateAfter = parseDate(value) }),\n   467â†’            (\"before:(\\\\d{4}-\\\\d{2}-\\\\d{2})\", { value, f in f.dateBefore = parseDate(value) }),\n   468â†’            (\"date:(\\\\d{4}-\\\\d{2}-\\\\d{2})\", { value, f in\n   469â†’                // date: sets both after and before to cover the whole day\n   470â†’                if let date = parseDate(value) {\n   471â†’                    f.dateAfter = date\n   472â†’                    // Set before to end of day\n   473â†’                    if let endOfDay = Calendar.current.date(byAdding: .day, value: 1, to: date) {\n   474â†’                        f.dateBefore = endOfDay\n   475â†’                    }\n   476â†’                }\n   477â†’            })\n   478â†’        ]\n   479â†’\n   480â†’        var remainingQuery = query\n   481â†’\n   482â†’        // Process each filter pattern\n   483â†’        for (pattern, handler) in filterPatterns {\n   484â†’            if let regex = try? NSRegularExpression(pattern: pattern, options: .caseInsensitive) {\n   485â†’                let range = NSRange(remainingQuery.startIndex..., in: remainingQuery)\n   486â†’                let matches = regex.matches(in: remainingQuery, options: [], range: range)\n   487â†’\n   488â†’                // Process matches in reverse to preserve indices\n   489â†’                for match in matches.reversed() {\n   490â†’                    // Extract captured group if present, otherwise use the whole match\n   491â†’                    let valueRange = match.numberOfRanges > 1 ? match.range(at: 1) : match.range\n   492â†’                    if let swiftRange = Range(valueRange, in: remainingQuery) {\n   493â†’                        let value = String(remainingQuery[swiftRange])\n   494â†’                        handler(value, &filter)\n   495â†’                    } else {\n   496â†’                        handler(\"\", &filter)\n   497â†’                    }\n   498â†’\n   499â†’                    // Remove matched text from query\n   500â†’                    if let swiftRange = Range(match.range, in: remainingQuery) {\n   501â†’                        remainingQuery.removeSubrange(swiftRange)\n   502â†’                    }\n   503â†’                }\n   504â†’            }\n   505â†’        }\n   506â†’\n   507â†’        // Clean up remaining query (free text for FTS)\n   508â†’        let cleanedFreeText = remainingQuery\n   509â†’            .trimmingCharacters(in: .whitespaces)\n   510â†’            .components(separatedBy: .whitespaces)\n   511â†’            .filter { !$0.isEmpty }\n   512â†’            .joined(separator: \" \")\n   513â†’\n   514â†’        if !cleanedFreeText.isEmpty {\n   515â†’            filter.freeText = cleanedFreeText\n   516â†’        }\n   517â†’\n   518â†’        return filter\n   519â†’    }\n   520â†’\n   521â†’    /// Search messages using structured filters and optional FTS\n   522â†’    public func searchMessagesWithFilters(_ filter: SearchFilter, limit: Int = 50, offset: Int = 0) throws -> [MailMessage] {\n   523â†’        guard let conn = connection else {\n   524â†’            throw MailDatabaseError.notInitialized\n   525â†’        }\n   526â†’\n   527â†’        var whereClauses: [String] = []\n   528â†’\n   529â†’        // Build WHERE clauses from filters\n   530â†’\n   531â†’        // from: filter (matches sender_name or sender_email)\n   532â†’        if let from = filter.from {\n   533â†’            let escaped = escapeSql(from)\n   534â†’            whereClauses.append(\"(m.sender_email LIKE '%\\(escaped)%' OR m.sender_name LIKE '%\\(escaped)%')\")\n   535â†’        }\n   536â†’\n   537â†’        // to: filter (requires join with recipients table)\n   538â†’        // For simplicity, we'll use a subquery\n   539â†’        if let to = filter.to {\n   540â†’            let escaped = escapeSql(to)\n   541â†’            whereClauses.append(\"\"\"\n   542â†’                EXISTS (SELECT 1 FROM recipients r WHERE r.message_id = m.id\n   543â†’                        AND (r.email LIKE '%\\(escaped)%' OR r.name LIKE '%\\(escaped)%'))\n   544â†’                \"\"\")\n   545â†’        }\n   546â†’\n   547â†’        // subject: filter\n   548â†’        if let subject = filter.subject {\n   549â†’            let escaped = escapeSql(subject)\n   550â†’            whereClauses.append(\"m.subject LIKE '%\\(escaped)%'\")\n   551â†’        }\n   552â†’\n   553â†’        // mailbox: filter\n   554â†’        if let mailbox = filter.mailbox {\n   555â†’            let escaped = escapeSql(mailbox)\n   556â†’            whereClauses.append(\"m.mailbox_name LIKE '%\\(escaped)%'\")\n   557â†’        }\n   558â†’\n   559â†’        // is:read / is:unread filter\n   560â†’        if let isRead = filter.isRead {\n   561â†’            whereClauses.append(\"m.is_read = \\(isRead ? 1 : 0)\")\n   562â†’        }\n   563â†’\n   564â†’        // is:flagged / is:unflagged filter\n   565â†’        if let isFlagged = filter.isFlagged {\n   566â†’            whereClauses.append(\"m.is_flagged = \\(isFlagged ? 1 : 0)\")\n   567â†’        }\n   568â†’\n   569â†’        // has:attachments filter\n   570â†’        if let hasAttachments = filter.hasAttachments, hasAttachments {\n   571â†’            whereClauses.append(\"m.has_attachments = 1\")\n   572â†’        }\n   573â†’\n   574â†’        // Date filters\n   575â†’        if let dateAfter = filter.dateAfter {\n   576â†’            let timestamp = Int(dateAfter.timeIntervalSince1970)\n   577â†’            whereClauses.append(\"m.date_received >= \\(timestamp)\")\n   578â†’        }\n   579â†’\n   580â†’        if let dateBefore = filter.dateBefore {\n   581â†’            let timestamp = Int(dateBefore.timeIntervalSince1970)\n   582â†’            whereClauses.append(\"m.date_received < \\(timestamp)\")\n   583â†’        }\n   584â†’\n   585â†’        // Mailbox status filter (inbox, archived, deleted)\n   586â†’        if let mailboxStatus = filter.mailboxStatus {\n   587â†’            whereClauses.append(\"m.mailbox_status = '\\(mailboxStatus.rawValue)'\")\n   588â†’        }\n   589â†’\n   590â†’        // Always exclude deleted messages\n   591â†’        whereClauses.append(\"m.is_deleted = 0\")\n   592â†’\n   593â†’        // Build the query\n   594â†’        var sql: String\n   595â†’\n   596â†’        if filter.hasFreeText, let freeText = filter.freeText {\n   597â†’            // Use FTS with additional filters\n   598â†’            let whereClause = whereClauses.isEmpty ? \"\" : \"AND \" + whereClauses.joined(separator: \" AND \")\n   599â†’            sql = \"\"\"\n   600â†’                SELECT m.* FROM messages m\n   601â†’                JOIN messages_fts fts ON m.rowid = fts.rowid\n   602â†’                WHERE messages_fts MATCH '\\(escapeSql(freeText))'\n   603â†’                \\(whereClause)\n   604â†’                ORDER BY bm25(messages_fts)\n   605â†’                LIMIT \\(limit) OFFSET \\(offset)\n   606â†’                \"\"\"\n   607â†’        } else if !whereClauses.isEmpty {\n   608â†’            // Filters only, no FTS\n   609â†’            let whereClause = whereClauses.joined(separator: \" AND \")\n   610â†’            sql = \"\"\"\n   611â†’                SELECT m.* FROM messages m\n   612â†’                WHERE \\(whereClause)\n   613â†’                ORDER BY m.date_received DESC\n   614â†’                LIMIT \\(limit) OFFSET \\(offset)\n   615â†’                \"\"\"\n   616â†’        } else {\n   617â†’            // No filters, return recent messages\n   618â†’            sql = \"\"\"\n   619â†’                SELECT * FROM messages m\n   620â†’                WHERE m.is_deleted = 0\n   621â†’                ORDER BY m.date_received DESC\n   622â†’                LIMIT \\(limit) OFFSET \\(offset)\n   623â†’                \"\"\"\n   624â†’        }\n   625â†’\n   626â†’        let result = try conn.query(sql)\n   627â†’\n   628â†’        var messages: [MailMessage] = []\n   629â†’        for row in result {\n   630â†’            if let message = try? rowToMessage(row) {\n   631â†’                messages.append(message)\n   632â†’            }\n   633â†’        }\n   634â†’        return messages\n   635â†’    }\n   636â†’\n   637â†’    /// Get all non-deleted messages (for export without search query)\n   638â†’    /// - Parameters:\n   639â†’    ///   - limit: Maximum number of messages to return\n   640â†’    ///   - offset: Number of messages to skip for pagination\n   641â†’    /// - Returns: Array of messages ordered by date received (newest first)\n   642â†’    public func getAllMessages(limit: Int = 100, offset: Int = 0) throws -> [MailMessage] {\n   643â†’        guard let conn = connection else {\n   644â†’            throw MailDatabaseError.notInitialized\n   645â†’        }\n   646â†’\n   647â†’        let query = \"\"\"\n   648â†’            SELECT id, apple_rowid, message_id, mailbox_id, subject, sender_name, sender_email,\n   649â†’                   recipients, date_sent, date_received, is_read, is_flagged, is_deleted,\n   650â†’                   body_text, body_html, emlx_path, export_path, created_at, updated_at\n   651â†’            FROM messages\n   652â†’            WHERE is_deleted = 0\n   653â†’            ORDER BY date_received DESC\n   654â†’            LIMIT \\(limit) OFFSET \\(offset)\n   655â†’            \"\"\"\n   656â†’\n   657â†’        let result = try conn.query(query)\n   658â†’        var messages: [MailMessage] = []\n   659â†’        for row in result {\n   660â†’            if let message = try? rowToMessage(row) {\n   661â†’                messages.append(message)\n   662â†’            }\n   663â†’        }\n   664â†’        return messages\n   665â†’    }\n   666â†’\n   667â†’    /// Get messages that haven't been exported yet (export_path IS NULL)\n   668â†’    /// Used for incremental auto-export after sync\n   669â†’    /// - Parameter limit: Maximum number of messages to return (default: unlimited via 0)\n   670â†’    /// - Returns: Array of messages that need to be exported\n   671â†’    public func getMessagesNeedingExport(limit: Int = 0) throws -> [MailMessage] {\n   672â†’        guard let conn = connection else {\n   673â†’            throw MailDatabaseError.notInitialized\n   674â†’        }\n   675â†’\n   676â†’        var query = \"\"\"\n   677â†’            SELECT id, apple_rowid, message_id, mailbox_id, subject, sender_name, sender_email,\n   678â†’                   recipients, date_sent, date_received, is_read, is_flagged, is_deleted,\n   679â†’                   body_text, body_html, emlx_path, export_path, created_at, updated_at\n   680â†’            FROM messages\n   681â†’            WHERE is_deleted = 0 AND export_path IS NULL\n   682â†’            ORDER BY date_received DESC\n   683â†’            \"\"\"\n   684â†’\n   685â†’        if limit > 0 {\n   686â†’            query += \" LIMIT \\(limit)\"\n   687â†’        }\n   688â†’\n   689â†’        let result = try conn.query(query)\n   690â†’        var messages: [MailMessage] = []\n   691â†’        for row in result {\n   692â†’            if let message = try? rowToMessage(row) {\n   693â†’                messages.append(message)\n   694â†’            }\n   695â†’        }\n   696â†’        return messages\n   697â†’    }\n   698â†’\n   699â†’    /// Update the export path for a message\n   700â†’    public func updateExportPath(id: String, path: String) throws {\n   701â†’        guard let conn = connection else {\n   702â†’            throw MailDatabaseError.notInitialized\n   703â†’        }\n   704â†’\n   705â†’        let now = Int(Date().timeIntervalSince1970)\n   706â†’        _ = try conn.execute(\"\"\"\n   707â†’            UPDATE messages SET export_path = '\\(escapeSql(path))', updated_at = \\(now)\n   708â†’            WHERE id = '\\(escapeSql(id))'\n   709â†’            \"\"\")\n   710â†’    }\n   711â†’\n   712â†’    /// Update the body content for a message (on-demand fetching)\n   713â†’    /// - Parameters:\n   714â†’    ///   - id: The message ID\n   715â†’    ///   - bodyText: Plain text body content (optional)\n   716â†’    ///   - bodyHtml: HTML body content (optional)\n   717â†’    public func updateMessageBody(id: String, bodyText: String?, bodyHtml: String?) throws {\n   718â†’        guard let conn = connection else {\n   719â†’            throw MailDatabaseError.notInitialized\n   720â†’        }\n   721â†’\n   722â†’        let now = Int(Date().timeIntervalSince1970)\n   723â†’        _ = try conn.execute(\"\"\"\n   724â†’            UPDATE messages SET\n   725â†’                body_text = \\(bodyText.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   726â†’                body_html = \\(bodyHtml.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   727â†’                updated_at = \\(now)\n   728â†’            WHERE id = '\\(escapeSql(id))'\n   729â†’            \"\"\")\n   730â†’    }\n   731â†’\n   732â†’    // MARK: - Sync Status Operations\n   733â†’\n   734â†’    /// Sync status key constants\n   735â†’    public enum SyncStatusKey {\n   736â†’        public static let state = \"sync_state\"\n   737â†’        public static let lastSyncTime = \"last_sync_time\"\n   738â†’        public static let lastSyncStartTime = \"last_sync_start_time\"\n   739â†’        public static let lastSyncEndTime = \"last_sync_end_time\"\n   740â†’        public static let lastSyncError = \"last_sync_error\"\n   741â†’        public static let lastSyncMessagesAdded = \"last_sync_messages_added\"\n   742â†’        public static let lastSyncMessagesUpdated = \"last_sync_messages_updated\"\n   743â†’        public static let lastSyncMessagesDeleted = \"last_sync_messages_deleted\"\n   744â†’        public static let lastSyncDuration = \"last_sync_duration\"\n   745â†’        public static let lastSyncIsIncremental = \"last_sync_is_incremental\"\n   746â†’    }\n   747â†’\n   748â†’    /// Sync state values\n   749â†’    public enum SyncState: String {\n   750â†’        case idle = \"idle\"\n   751â†’        case running = \"running\"\n   752â†’        case success = \"success\"\n   753â†’        case failed = \"failed\"\n   754â†’    }\n   755â†’\n   756â†’    /// Summary of sync status for reporting\n   757â†’    public struct SyncStatusSummary {\n   758â†’        public let state: SyncState\n   759â†’        public let lastSyncTime: Date?\n   760â†’        public let lastSyncStartTime: Date?\n   761â†’        public let lastSyncEndTime: Date?\n   762â†’        public let lastSyncError: String?\n   763â†’        public let messagesAdded: Int\n   764â†’        public let messagesUpdated: Int\n   765â†’        public let messagesDeleted: Int\n   766â†’        public let duration: TimeInterval?\n   767â†’        public let isIncremental: Bool?\n   768â†’\n   769â†’        public init(\n   770â†’            state: SyncState,\n   771â†’            lastSyncTime: Date? = nil,\n   772â†’            lastSyncStartTime: Date? = nil,\n   773â†’            lastSyncEndTime: Date? = nil,\n   774â†’            lastSyncError: String? = nil,\n   775â†’            messagesAdded: Int = 0,\n   776â†’            messagesUpdated: Int = 0,\n   777â†’            messagesDeleted: Int = 0,\n   778â†’            duration: TimeInterval? = nil,\n   779â†’            isIncremental: Bool? = nil\n   780â†’        ) {\n   781â†’            self.state = state\n   782â†’            self.lastSyncTime = lastSyncTime\n   783â†’            self.lastSyncStartTime = lastSyncStartTime\n   784â†’            self.lastSyncEndTime = lastSyncEndTime\n   785â†’            self.lastSyncError = lastSyncError\n   786â†’            self.messagesAdded = messagesAdded\n   787â†’            self.messagesUpdated = messagesUpdated\n   788â†’            self.messagesDeleted = messagesDeleted\n   789â†’            self.duration = duration\n   790â†’            self.isIncremental = isIncremental\n   791â†’        }\n   792â†’    }\n   793â†’\n   794â†’    /// Get a sync status value\n   795â†’    public func getSyncStatus(key: String) throws -> String? {\n   796â†’        guard let conn = connection else {\n   797â†’            throw MailDatabaseError.notInitialized\n   798â†’        }\n   799â†’\n   800â†’        let result = try conn.query(\"SELECT value FROM sync_status WHERE key = '\\(escapeSql(key))'\")\n   801â†’\n   802â†’        for row in result {\n   803â†’            return try row.getString(0)\n   804â†’        }\n   805â†’        return nil\n   806â†’    }\n   807â†’\n   808â†’    /// Set a sync status value\n   809â†’    public func setSyncStatus(key: String, value: String) throws {\n   810â†’        guard let conn = connection else {\n   811â†’            throw MailDatabaseError.notInitialized\n   812â†’        }\n   813â†’\n   814â†’        let now = Int(Date().timeIntervalSince1970)\n   815â†’        _ = try conn.execute(\"\"\"\n   816â†’            INSERT INTO sync_status (key, value, updated_at)\n   817â†’            VALUES ('\\(escapeSql(key))', '\\(escapeSql(value))', \\(now))\n   818â†’            ON CONFLICT(key) DO UPDATE SET\n   819â†’                value = excluded.value,\n   820â†’                updated_at = excluded.updated_at\n   821â†’            \"\"\")\n   822â†’    }\n   823â†’\n   824â†’    /// Get the last sync timestamp\n   825â†’    public func getLastSyncTime() throws -> Date? {\n   826â†’        if let value = try getSyncStatus(key: \"last_sync_time\"),\n   827â†’           let timestamp = Double(value) {\n   828â†’            return Date(timeIntervalSince1970: timestamp)\n   829â†’        }\n   830â†’        return nil\n   831â†’    }\n   832â†’\n   833â†’    /// Set the last sync timestamp\n   834â†’    public func setLastSyncTime(_ date: Date) throws {\n   835â†’        try setSyncStatus(key: SyncStatusKey.lastSyncTime, value: String(date.timeIntervalSince1970))\n   836â†’    }\n   837â†’\n   838â†’    /// Record that a sync operation has started\n   839â†’    public func recordSyncStart(isIncremental: Bool) throws {\n   840â†’        let now = Date()\n   841â†’        try setSyncStatus(key: SyncStatusKey.state, value: SyncState.running.rawValue)\n   842â†’        try setSyncStatus(key: SyncStatusKey.lastSyncStartTime, value: String(now.timeIntervalSince1970))\n   843â†’        try setSyncStatus(key: SyncStatusKey.lastSyncIsIncremental, value: isIncremental ? \"1\" : \"0\")\n   844â†’        // Clear previous error\n   845â†’        try setSyncStatus(key: SyncStatusKey.lastSyncError, value: \"\")\n   846â†’    }\n   847â†’\n   848â†’    /// Record successful sync completion\n   849â†’    public func recordSyncSuccess(result: SyncResult) throws {\n   850â†’        let now = Date()\n   851â†’        try setSyncStatus(key: SyncStatusKey.state, value: SyncState.success.rawValue)\n   852â†’        try setSyncStatus(key: SyncStatusKey.lastSyncEndTime, value: String(now.timeIntervalSince1970))\n   853â†’        try setSyncStatus(key: SyncStatusKey.lastSyncTime, value: String(now.timeIntervalSince1970))\n   854â†’        try setSyncStatus(key: SyncStatusKey.lastSyncMessagesAdded, value: String(result.messagesAdded))\n   855â†’        try setSyncStatus(key: SyncStatusKey.lastSyncMessagesUpdated, value: String(result.messagesUpdated))\n   856â†’        try setSyncStatus(key: SyncStatusKey.lastSyncMessagesDeleted, value: String(result.messagesDeleted))\n   857â†’        try setSyncStatus(key: SyncStatusKey.lastSyncDuration, value: String(result.duration))\n   858â†’        try setSyncStatus(key: SyncStatusKey.lastSyncError, value: \"\")\n   859â†’    }\n   860â†’\n   861â†’    /// Record sync failure\n   862â†’    public func recordSyncFailure(error: Error) throws {\n   863â†’        let now = Date()\n   864â†’        try setSyncStatus(key: SyncStatusKey.state, value: SyncState.failed.rawValue)\n   865â†’        try setSyncStatus(key: SyncStatusKey.lastSyncEndTime, value: String(now.timeIntervalSince1970))\n   866â†’        try setSyncStatus(key: SyncStatusKey.lastSyncError, value: error.localizedDescription)\n   867â†’    }\n   868â†’\n   869â†’    /// Get a summary of the current sync status\n   870â†’    public func getSyncStatusSummary() throws -> SyncStatusSummary {\n   871â†’        let stateStr = try getSyncStatus(key: SyncStatusKey.state)\n   872â†’        let state = stateStr.flatMap { SyncState(rawValue: $0) } ?? .idle\n   873â†’\n   874â†’        let lastSyncTime = try getSyncStatus(key: SyncStatusKey.lastSyncTime)\n   875â†’            .flatMap { Double($0) }\n   876â†’            .map { Date(timeIntervalSince1970: $0) }\n   877â†’\n   878â†’        let lastSyncStartTime = try getSyncStatus(key: SyncStatusKey.lastSyncStartTime)\n   879â†’            .flatMap { Double($0) }\n   880â†’            .map { Date(timeIntervalSince1970: $0) }\n   881â†’\n   882â†’        let lastSyncEndTime = try getSyncStatus(key: SyncStatusKey.lastSyncEndTime)\n   883â†’            .flatMap { Double($0) }\n   884â†’            .map { Date(timeIntervalSince1970: $0) }\n   885â†’\n   886â†’        let lastSyncError = try getSyncStatus(key: SyncStatusKey.lastSyncError)\n   887â†’            .flatMap { $0.isEmpty ? nil : $0 }\n   888â†’\n   889â†’        let messagesAdded = try getSyncStatus(key: SyncStatusKey.lastSyncMessagesAdded)\n   890â†’            .flatMap { Int($0) } ?? 0\n   891â†’\n   892â†’        let messagesUpdated = try getSyncStatus(key: SyncStatusKey.lastSyncMessagesUpdated)\n   893â†’            .flatMap { Int($0) } ?? 0\n   894â†’\n   895â†’        let messagesDeleted = try getSyncStatus(key: SyncStatusKey.lastSyncMessagesDeleted)\n   896â†’            .flatMap { Int($0) } ?? 0\n   897â†’\n   898â†’        let duration = try getSyncStatus(key: SyncStatusKey.lastSyncDuration)\n   899â†’            .flatMap { Double($0) }\n   900â†’\n   901â†’        let isIncremental = try getSyncStatus(key: SyncStatusKey.lastSyncIsIncremental)\n   902â†’            .map { $0 == \"1\" }\n   903â†’\n   904â†’        return SyncStatusSummary(\n   905â†’            state: state,\n   906â†’            lastSyncTime: lastSyncTime,\n   907â†’            lastSyncStartTime: lastSyncStartTime,\n   908â†’            lastSyncEndTime: lastSyncEndTime,\n   909â†’            lastSyncError: lastSyncError,\n   910â†’            messagesAdded: messagesAdded,\n   911â†’            messagesUpdated: messagesUpdated,\n   912â†’            messagesDeleted: messagesDeleted,\n   913â†’            duration: duration,\n   914â†’            isIncremental: isIncremental\n   915â†’        )\n   916â†’    }\n   917â†’\n   918â†’    // MARK: - Mailbox Operations\n   919â†’\n   920â†’    /// Upsert a mailbox\n   921â†’    public func upsertMailbox(_ mailbox: Mailbox) throws {\n   922â†’        guard let conn = connection else {\n   923â†’            throw MailDatabaseError.notInitialized\n   924â†’        }\n   925â†’\n   926â†’        let now = Int(Date().timeIntervalSince1970)\n   927â†’        _ = try conn.execute(\"\"\"\n   928â†’            INSERT INTO mailboxes (id, account_id, name, full_path, parent_id, message_count, unread_count, synced_at)\n   929â†’            VALUES (\\(mailbox.id), '\\(escapeSql(mailbox.accountId))', '\\(escapeSql(mailbox.name))',\n   930â†’                    '\\(escapeSql(mailbox.fullPath))', \\(mailbox.parentId ?? 0), \\(mailbox.messageCount),\n   931â†’                    \\(mailbox.unreadCount), \\(now))\n   932â†’            ON CONFLICT(id) DO UPDATE SET\n   933â†’                account_id = excluded.account_id,\n   934â†’                name = excluded.name,\n   935â†’                full_path = excluded.full_path,\n   936â†’                parent_id = excluded.parent_id,\n   937â†’                message_count = excluded.message_count,\n   938â†’                unread_count = excluded.unread_count,\n   939â†’                synced_at = excluded.synced_at\n   940â†’            \"\"\")\n   941â†’    }\n   942â†’\n   943â†’    /// Get all mailboxes\n   944â†’    public func getMailboxes() throws -> [Mailbox] {\n   945â†’        guard let conn = connection else {\n   946â†’            throw MailDatabaseError.notInitialized\n   947â†’        }\n   948â†’\n   949â†’        let result = try conn.query(\"SELECT * FROM mailboxes ORDER BY full_path\")\n   950â†’\n   951â†’        var mailboxes: [Mailbox] = []\n   952â†’        for row in result {\n   953â†’            if let mailbox = try? rowToMailbox(row) {\n   954â†’                mailboxes.append(mailbox)\n   955â†’            }\n   956â†’        }\n   957â†’        return mailboxes\n   958â†’    }\n   959â†’\n   960â†’    // MARK: - FTS5 Trigger Management\n   961â†’\n   962â†’    /// Disable FTS5 triggers to prevent per-row FTS updates during bulk operations.\n   963â†’    /// Call `rebuildFTSIndex()` after bulk operations complete to sync the FTS index.\n   964â†’    public func disableFTSTriggers() throws {\n   965â†’        guard let conn = connection else {\n   966â†’            throw MailDatabaseError.notInitialized\n   967â†’        }\n   968â†’\n   969â†’        _ = try conn.execute(\"DROP TRIGGER IF EXISTS messages_ai\")\n   970â†’        _ = try conn.execute(\"DROP TRIGGER IF EXISTS messages_ad\")\n   971â†’        _ = try conn.execute(\"DROP TRIGGER IF EXISTS messages_au\")\n   972â†’    }\n   973â†’\n   974â†’    /// Re-enable FTS5 triggers for incremental updates after bulk operations.\n   975â†’    public func enableFTSTriggers() throws {\n   976â†’        guard let conn = connection else {\n   977â†’            throw MailDatabaseError.notInitialized\n   978â†’        }\n   979â†’\n   980â†’        // Recreate the triggers (same as in migration)\n   981â†’        _ = try conn.execute(\"\"\"\n   982â†’            CREATE TRIGGER IF NOT EXISTS messages_ai AFTER INSERT ON messages BEGIN\n   983â†’                INSERT INTO messages_fts(rowid, subject, sender_name, sender_email, body_text)\n   984â†’                VALUES (NEW.rowid, NEW.subject, NEW.sender_name, NEW.sender_email, NEW.body_text);\n   985â†’            END\n   986â†’            \"\"\")\n   987â†’\n   988â†’        _ = try conn.execute(\"\"\"\n   989â†’            CREATE TRIGGER IF NOT EXISTS messages_ad AFTER DELETE ON messages BEGIN\n   990â†’                INSERT INTO messages_fts(messages_fts, rowid, subject, sender_name, sender_email, body_text)\n   991â†’                VALUES ('delete', OLD.rowid, OLD.subject, OLD.sender_name, OLD.sender_email, OLD.body_text);\n   992â†’            END\n   993â†’            \"\"\")\n   994â†’\n   995â†’        _ = try conn.execute(\"\"\"\n   996â†’            CREATE TRIGGER IF NOT EXISTS messages_au AFTER UPDATE ON messages BEGIN\n   997â†’                INSERT INTO messages_fts(messages_fts, rowid, subject, sender_name, sender_email, body_text)\n   998â†’                VALUES ('delete', OLD.rowid, OLD.subject, OLD.sender_name, OLD.sender_email, OLD.body_text);\n   999â†’                INSERT INTO messages_fts(rowid, subject, sender_name, sender_email, body_text)\n  1000â†’                VALUES (NEW.rowid, NEW.subject, NEW.sender_name, NEW.sender_email, NEW.body_text);\n  1001â†’            END\n  1002â†’            \"\"\")\n  1003â†’    }\n  1004â†’\n  1005â†’    /// Rebuild the FTS5 index from scratch using the messages table content.\n  1006â†’    /// This is much faster than per-row trigger updates for bulk operations.\n  1007â†’    /// Should be called after `disableFTSTriggers()` + bulk operations.\n  1008â†’    public func rebuildFTSIndex() throws {\n  1009â†’        guard let conn = connection else {\n  1010â†’            throw MailDatabaseError.notInitialized\n  1011â†’        }\n  1012â†’\n  1013â†’        // Delete all FTS content and rebuild from the messages table\n  1014â†’        // The 'rebuild' command is a special FTS5 command that repopulates from the content table\n  1015â†’        _ = try conn.execute(\"INSERT INTO messages_fts(messages_fts) VALUES('rebuild')\")\n  1016â†’    }\n  1017â†’\n  1018â†’    // MARK: - Batch Insert Operations\n  1019â†’\n  1020â†’    /// Configuration for batch insert operations\n  1021â†’    public struct BatchInsertConfig: Sendable {\n  1022â†’        /// Number of messages to insert per transaction batch\n  1023â†’        public let batchSize: Int\n  1024â†’\n  1025â†’        /// Whether to disable FTS triggers during bulk operations (recommended for large syncs)\n  1026â†’        public let disableFTSTriggersForBulk: Bool\n  1027â†’\n  1028â†’        /// Default configuration with batch size of 1000 and FTS trigger optimization enabled\n  1029â†’        public static let `default` = BatchInsertConfig(batchSize: 1000, disableFTSTriggersForBulk: true)\n  1030â†’\n  1031â†’        public init(batchSize: Int = 1000, disableFTSTriggersForBulk: Bool = true) {\n  1032â†’            self.batchSize = max(1, batchSize)  // Ensure at least 1\n  1033â†’            self.disableFTSTriggersForBulk = disableFTSTriggersForBulk\n  1034â†’        }\n  1035â†’    }\n  1036â†’\n  1037â†’    /// Result of a batch insert o