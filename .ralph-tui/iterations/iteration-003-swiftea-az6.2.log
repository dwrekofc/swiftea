             is_read, is_flagged, is_deleted, has_attachments, emlx_path,\n   301â†’                body_text, body_html, synced_at, updated_at,\n   302â†’                mailbox_status, pending_sync_action, last_known_mailbox_id,\n   303â†’                in_reply_to, threading_references\n   304â†’            ) VALUES (\n   305â†’                '\\(message.id)', \\(message.appleRowId ?? 0), \\(message.messageId.map { \"'\\($0)'\" } ?? \"NULL\"),\n   306â†’                \\(message.mailboxId ?? 0), \\(message.mailboxName.map { \"'\\($0)'\" } ?? \"NULL\"),\n   307â†’                \\(message.accountId.map { \"'\\($0)'\" } ?? \"NULL\"),\n   308â†’                '\\(escapeSql(message.subject))', \\(message.senderName.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   309â†’                \\(message.senderEmail.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   310â†’                \\(message.dateSent.map { Int($0.timeIntervalSince1970) } ?? 0),\n   311â†’                \\(message.dateReceived.map { Int($0.timeIntervalSince1970) } ?? 0),\n   312â†’                \\(message.isRead ? 1 : 0), \\(message.isFlagged ? 1 : 0), \\(message.isDeleted ? 1 : 0),\n   313â†’                \\(message.hasAttachments ? 1 : 0), \\(message.emlxPath.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   314â†’                \\(message.bodyText.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   315â†’                \\(message.bodyHtml.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   316â†’                \\(now), \\(now),\n   317â†’                '\\(message.mailboxStatus.rawValue)',\n   318â†’                \\(message.pendingSyncAction.map { \"'\\($0.rawValue)'\" } ?? \"NULL\"),\n   319â†’                \\(message.lastKnownMailboxId.map { String($0) } ?? \"NULL\"),\n   320â†’                \\(message.inReplyTo.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\"),\n   321â†’                \\(referencesJson.map { \"'\\(escapeSql($0))'\" } ?? \"NULL\")\n   322â†’            )\n   323â†’    