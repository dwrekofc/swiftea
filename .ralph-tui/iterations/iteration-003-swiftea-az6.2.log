1076â†’\n  1077â†’        let now = Int(Date().timeIntervalSince1970)\n  1078â†’\n  1079â†’        // Disable FTS triggers for bulk operations to prevent per-row FTS updates\n  1080â†’        // This is critical for performance: without this, second sync takes 5+ minutes\n  1081â†’        // because each UPDATE fires 2 FTS operations (delete old + insert new)\n  1082â†’        if config.disableFTSTriggersForBulk {\n  1083â†’            try disableFTSTriggers()\n  1084â†’        }\n  1085â†’\n  1086â†’        defer {\n  1087â†’            // Always re-enable triggers and rebuild FTS index\n  1088â†’            if config.disableFTSTriggersForBulk {\n  1089â†’                do {\n  1090â†’                    try rebuildFTSIndex()\n  1091â†’                    try enableFTSTriggers()\n  1092â†’                } catch {\n  1093â†’                    errors.append(\"FTS rebuild failed: \\(error.localizedDescription)\")\n  1094â†’                }\n  1095â†’            }\n  1096â†’        }\n  1097â†’\n  1098â†’        // Process in batches\n  1099â†’        let batches = stride(from: 0, to: messages.count, by: config.batchSize).map { startIndex in\n  1100â†’            let endIndex = min(startIndex + config.batchSize, messages.count)\n  1101â†’            return Array(messages[startIndex..<endIndex])\n  1102â†’        }\n  1103â†’\n  1104â†’        for (batchIndex, batch) in batches.enumerated() {\n  1105â†’            do {\n  1106â†’                let (inserted, updated) = try executeBatchTransaction(batch, connection: conn, timestamp: now)\n  1107â†’                totalInserted += inserted\n  1108â†’                totalUpdated += updated\n  1109â†’            } catch {\n  1110â†’                // Batch failed - record error and count all messages in batch as failed\n  1111â†’                totalFailed += batch.count\n  1112â†’                errors.append(\"Batch \\(batchIndex + 1) failed: \\(error.localizedDescription)\")\n  1113â†’            }\n  1114â†’       