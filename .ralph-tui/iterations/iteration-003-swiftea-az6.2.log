{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_017qq24njFwZu4v1xLMcWi69","type":"tool_result","content":"     1â†’// MailDatabase - libSQL mirror database for Apple Mail data\n     2â†’\n     3â†’import Foundation\n     4â†’import Libsql\n     5â†’\n     6â†’/// Errors that can occur during mail database operations\n     7â†’public enum MailDatabaseError: Error, LocalizedError {\n     8â†’    case connectionFailed(underlying: Error)\n     9â†’    case migrationFailed(underlying: Error)\n    10â†’    case queryFailed(underlying: Error)\n    11â†’    case notInitialized\n    12â†’\n    13â†’    public var errorDescription: String? {\n    14â†’        switch self {\n    15â†’        case .connectionFailed(let error):\n    16â†’            return \"Failed to connect to mail database: \\(error.localizedDescription)\"\n    17â†’        case .migrationFailed(let error):\n    18â†’            return \"Failed to run database migration: \\(error.localizedDescription)\"\n    19â†’        case .queryFailed(let error):\n    20â†’            return \"Database query failed: \\(error.localizedDescription)\"\n    21â†’        case .notInitialized:\n    22â†’            return \"Mail database not initialized\"\n    23â†’        }\n    24â†’    }\n    25â†’}\n    26â†’\n    27â†’/// Manages the libSQL mirror database for Apple Mail data\n    28â†’public final class MailDatabase: @unchecked Sendable {\n    29â†’    private var database: Database?\n    30â†’    private var connection: Connection?\n    31â†’    private let databasePath: String\n    32â†’\n    33â†’    public init(databasePath: String) {\n    34â†’        self.databasePath = databasePath\n    35â†’    }\n    36â†’\n    37â†’    /// Initialize the database connection and run migrations\n    38â†’    public func initialize() throws {\n    39â†’        do {\n    40â†’            database = try Database(databasePath)\n    41â†’            connection = try database?.connect()\n    42â†’\n    43â†’            // Enable WAL mode for concurrent access (allows readers while writing)\n    44â†’            // This prevents \"database is locked\" errors when daemon and manual sync run concurrently\n    45â†’            if let conn = connection {\n    46â†’                _ = try conn.query(\"PRAGMA journal_mode=WAL\")\n    47â†’                // Set busy timeout to 5 seconds to wait for locks instead of failing immediately\n    48â†’                _ = try conn.query(\"PRAGMA busy_timeout=5000\")\n    49â†’            }\n    50â†’\n    51â†’            try runMigrations()\n    52â†’        } catch {\n    53â†’            throw MailDatabaseError.connectionFailed(underlying: error)\n    54â†’        }\n    55â†’    }\n    56â†’\n    57â†’    /// Close the database connection\n    58â†’    public func close() {\n    59â†’        connection = nil\n    60â†’        database = nil\n    61â†’    }\n    62â†’\n    63â†’    // MARK: - Schema Definition\n    64â†’\n    65â†’    /// Run all pending migrations to bring schema up to date\n    66â†’    private func runMigrations() throws {\n    67â†’        guard let conn = connection else {\n    68â†’            throw MailDatabaseError.notInitialized\n    69â†’        }\n    70â†’\n    71â†’        // Create schema version tracking table\n    72â†’        _ = try conn.execute(\"\"\"\n    73â†’            CREATE TABLE IF NOT EXISTS schema_version (\n    74â†’                version INTEGER PRIMARY KEY,\n    75â†’                applied_at TEXT NOT NULL DEFAULT (datetime('now'))\n    76â†’            )\n    77â†’            \"\"\")\n    78â†’\n    79â†’        // Get current version\n    80â†’        let result = try conn.query(\"SELECT MAX(version) as v FROM schema_version\")\n    81â†’        var currentVersion = 0\n    82â†’        for row in result {\n    83â†’            if let v = try? row.getInt(0) {\n    84â†’                currentVersion = v\n    85â†’            }\n    86â†’        }\n    87â†’\n    88â†’        // Apply migrations\n    89â†’        if currentVersion < 1 {\n    90â†’            try applyMigrationV1(conn)\n    91â†’        }\n    92â†’        if currentVersion < 2 {\n    93â†’            try applyMigrationV2(conn)\n    94â†’        }\n    95â†’        if currentVersion < 3 {\n    96â†’            try applyMigrationV3(conn)\n    97â†’        }\n    98â†’    }\n    99â†’\n   100â†’    /// Migration v1: Initial schema with all mail tables\n   101â†’    private func applyMigrationV1(_ conn: Connection) throws {\n   102â†’        do {\n   103â†’            // Messages table - core email metadata\n   104â†’            _ = try conn.execute(\"\"\"\n   105â†’                CREATE TABLE IF NOT EXISTS messages (\n   106â†’                    id TEXT PRIMARY KEY,\n   107â†’                    apple_rowid INTEGER,\n   108â†’                    message_id TEXT,\n   109â†’                    mailbox_id INTEGER,\n   110â†’                    mailbox_name TEXT,\n   111â†’                    account_id TEXT,\n   112â†’                    subject TEXT,\n   113â†’                    sender_name TEXT,\n   114â†’                    sender_email TEXT,\n   115â†’                    date_sent INTEGER,\n   116â†’                    date_received INTEGER,\n   117â†’                    is_read INTEGER DEFAULT 0,\n   118â†’                    is_flagged INTEGER DEFAULT 0,\n   119â†’                    is_deleted INTEGER DEFAULT 0,\n   120â†’                    has_attachments INTEGER DEFAULT 0,\n   121â†’                    emlx_path TEXT,\n   122â†’                    body_text TEXT,\n   123â†’                    body_html TEXT,\n   124â†’                    export_path TEXT,\n   125â†’                    synced_at INTEGER NOT NULL,\n   126â†’                    updated_at INTEGER NOT NULL\n   127â†’                )\n   128â†’                \"\"\")\n   129â†’\n   130â†’            // Recipients table - to/cc/bcc addresses\n   131â†’            _ = try conn.execute(\"\"\"\n   132â†’                CREATE TABLE IF NOT EXISTS recipients (\n   133â†’                    id INTEGER PRIMARY KEY AUTOINCREMENT,\n   134â†’                    message_id TEXT NOT NULL,\n   135â†’                    type TEXT NOT NULL,\n   136â†’                    name TEXT,\n   137â†’                    email TEXT NOT NULL,\n   138â†’                    FOREIGN KEY (message_id) REFERENCES messages(id) ON DELETE CASCADE\n   139â†’                )\n   140â†’                \"\"\")\n   141â†’\n   142â†’            // Attachments table - attachment metadata\n   143â†’            _ = try conn.execute(\"\"\"\n   144â†’                CREATE TABLE IF NOT EXISTS attachments (\n   145â†’                    id INTEGER PRIMARY KEY AUTOINCREMENT,\n   146â†’                    message_id TEXT NOT NULL,\n   147â†’                    filename TEXT NOT NULL,\n   148â†’                    mime_type TEXT,\n   149â†’                    size INTEGER,\n   150â†’                    content_id TEXT,\n   151â†’                    is_inline INTEGER DEFAULT 0,\n   152â†’                    FOREIGN KEY (message_id) REFERENCES messages(id) ON DELETE CASCADE\n   153â†’                )\n   154â†’                \"\"\")\n   155â†’\n   156â†’            // Mailboxes table - folder hierarchy\n   157â†’            _ = try conn.execute(\"\"\"\n   158â†’                CREATE TABLE IF NOT EXISTS mailboxes (\n   159â†’                    id INTEGER PRIMARY KEY,\n   160â†’                    account_id TEXT NOT NULL,\n   161â†’                    name TEXT NOT NULL,\n   162â†’                    full_path TEXT NOT NULL,\n   163â†’                    parent_id INTEGER,\n   164â†’                    message_count INTEGER DEFAULT 0,\n   165â†’                    unread_count INTEGER DEFAULT 0,\n   166â†’                    synced_at INTEGER NOT NULL\n   167â†’                )\n   168â†’                \"\"\")\n   169â†’\n   170â†’            // Sync status table - track sync progress\n   171â†’            _ = try conn.execute(\"\"\"\n   172â†’                CREATE TABLE IF NOT EXISTS sync_status (\n   173â†’                    key TEXT PRIMARY KEY,\n   174â†’                    value TEXT NOT NULL,\n   175â†’                    updated_at INTEGER NOT NULL\n   176â†’                )\n   177â†’                \"\"\")\n   178â†’\n   179â†’            // Create indexes for common lookups\n   180â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_apple_rowid ON messages(apple_rowid)\")\n   181â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_message_id ON messages(message_id)\")\n   182â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_mailbox_id ON messages(mailbox_id)\")\n   183â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_account_id ON messages(account_id)\")\n   184â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_date_received ON messages(date_received)\")\n   185â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_is_read ON messages(is_read)\")\n   186â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_is_flagged ON messages(is_flagged)\")\n   187â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_recipients_message_id ON recipients(message_id)\")\n   188â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_attachments_message_id ON attachments(message_id)\")\n   189â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_mailboxes_account_id ON mailboxes(account_id)\")\n   190â†’\n   191â†’            // Create FTS5 virtual table for full-text search\n   192â†’            // Note: Using external content table (content='messages') means FTS reads from messages table\n   193â†’            _ = try conn.execute(\"\"\"\n   194â†’                CREATE VIRTUAL TABLE IF NOT EXISTS messages_fts USING fts5(\n   195â†’                    subject,\n   196â†’                    sender_name,\n   197â†’                    sender_email,\n   198â†’                    body_text,\n   199â†’                    content='messages',\n   200â†’                    content_rowid='rowid',\n   201â†’                    tokenize='porter unicode61'\n   202â†’                )\n   203â†’                \"\"\")\n   204â†’\n   205â†’            // Create triggers to keep FTS index in sync\n   206â†’            _ = try conn.execute(\"\"\"\n   207â†’                CREATE TRIGGER IF NOT EXISTS messages_ai AFTER INSERT ON messages BEGIN\n   208â†’                    INSERT INTO messages_fts(rowid, subject, sender_name, sender_email, body_text)\n   209â†’                    VALUES (NEW.rowid, NEW.subject, NEW.sender_name, NEW.sender_email, NEW.body_text);\n   210â†’                END\n   211â†’                \"\"\")\n   212â†’\n   213â†’            _ = try conn.execute(\"\"\"\n   214â†’                CREATE TRIGGER IF NOT EXISTS messages_ad AFTER DELETE ON messages BEGIN\n   215â†’                    INSERT INTO messages_fts(messages_fts, rowid, subject, sender_name, sender_email, body_text)\n   216â†’                    VALUES ('delete', OLD.rowid, OLD.subject, OLD.sender_name, OLD.sender_email, OLD.body_text);\n   217â†’                END\n   218â†’                \"\"\")\n   219â†’\n   220â†’            _ = try conn.execute(\"\"\"\n   221â†’                CREATE TRIGGER IF NOT EXISTS messages_au AFTER UPDATE ON messages BEGIN\n   222â†’                    INSERT INTO messages_fts(messages_fts, rowid, subject, sender_name, sender_email, body_text)\n   223â†’                    VALUES ('delete', OLD.rowid, OLD.subject, OLD.sender_name, OLD.sender_email, OLD.body_text);\n   224â†’                    INSERT INTO messages_fts(rowid, subject, sender_name, sender_email, body_text)\n   225â†’                    VALUES (NEW.rowid, NEW.subject, NEW.sender_name, NEW.sender_email, NEW.body_text);\n   226â†’                END\n   227â†’                \"\"\")\n   228â†’\n   229â†’            // Record migration version\n   230â†’            _ = try conn.execute(\"INSERT INTO schema_version (version) VALUES (1)\")\n   231â†’\n   232â†’        } catch {\n   233â†’            throw MailDatabaseError.migrationFailed(underlying: error)\n   234â†’        }\n   235â†’    }\n   236â†’\n   237â†’    /// Migration v2: Add mailbox status tracking columns for bidirectional sync\n   238â†’    private func applyMigrationV2(_ conn: Connection) throws {\n   239â†’        do {\n   240â†’            // Add mailbox_status column with default 'inbox' for existing messages\n   241â†’            _ = try conn.execute(\"ALTER TABLE messages ADD COLUMN mailbox_status TEXT DEFAULT 'inbox'\")\n   242â†’\n   243â†’            // Add pending_sync_action column (nullable - only set when action is pending)\n   244â†’            _ = try conn.execute(\"ALTER TABLE messages ADD COLUMN pending_sync_action TEXT\")\n   245â†’\n   246â†’            // Add last_known_mailbox_id column (nullable - tracks original mailbox for move detection)\n   247â†’            _ = try conn.execute(\"ALTER TABLE messages ADD COLUMN last_known_mailbox_id INTEGER\")\n   248â†’\n   249â†’            // Create index for querying messages by mailbox status\n   250â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_mailbox_status ON messages(mailbox_status)\")\n   251â†’\n   252â†’            // Create index for finding messages with pending sync actions\n   253â†’            _ = try conn.execute(\"CREATE INDEX IF NOT EXISTS idx_messages_pending_sync_action ON messages(pending_sync_action)\")\n   254â†’\n   255â†’            // Record migration version\n   256â†’            _ = try conn.execute(\"INSERT INTO schema_version (version) VALUES (2)\")\n   257â†’\n   258â†’        } catch {\n   259â†’            throw MailDatabaseError.migrationFailed(underlying: error)\n   260â†’        }\n   261â†’    }\n   262â†’\n   263â†’    /// Migration