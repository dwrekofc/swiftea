      var exportedCount = 0\n  1627â†’        var attachmentCount = 0\n  1628â†’        let emlxParser = EmlxParser()\n  1629â†’\n  1630â†’        for message in messages {\n  1631â†’            let filename = generateFilename(for: message)\n  1632â†’            let filePath = (outputDir as NSString).appendingPathComponent(filename)\n  1633â†’\n  1634â†’            let content: String\n  1635â†’            if format.isJson {\n  1636â†’                content = formatAsJson(message)\n  1637â†’            } else {\n  1638â†’                content = formatAsMarkdown(message)\n  1639â†’            }\n  1640â†’\n  1641â†’            try content.write(toFile: filePath, atomically: true, encoding: .utf8)\n  1642â†’\n  1643â†’            // Update export path in database\n  1644â†’            try mailDatabase.updateExportPath(id: message.id, path: filePath)\n  1645â†’            exportedCount += 1\n  1646â†’\n  1647â†’            // Extract attachments if requested\n  1648â†’            if includeAttachments, message.hasAttachments, let emlxPath = message.emlxPath {\n  1649â†’                // Skip EWS/Exchange messages (no local .emlx file)\n  1650â†’                if emlxPath.contains(\"ews:\") || emlxPath.contains(\"/ews:/\") {\n  1651â†’                    continue\n  1652â†’                }\n  1653â†’\n  1654â†’                let attachmentsDir = (outputDir as NSString).appendingPathComponent(\"attachments/\\(message.id)\")\n  1655â†’\n  1656â†’                do {\n  1657â†’                    let attachments = try emlxParser.extractAttachments(path: emlxPath)\n  1658â†’\n  1659â†’                    if !attachments.isEmpty {\n  1660â†’                        // Create attachments directory\n  1661â†’                        if !fileManager.fileExists(atPath: attachmentsDir) {\n  1662â†’                            try fileManager.createDirectory(atPath: attachmentsDir, withIntermediateDirectories: true)\n  1663â†’                        }\n  1664â†’\n  1665â†’    