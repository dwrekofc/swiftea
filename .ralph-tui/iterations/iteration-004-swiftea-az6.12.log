{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_011fN8bB9AwsXjaUHJ9UyQpp","type":"tool_result","content":"     1â†’import Foundation\n     2â†’import SwiftEAKit\n     3â†’\n     4â†’// MARK: - Mail Command Utilities\n     5â†’\n     6â†’/// Shared utility functions for mail commands.\n     7â†’/// These functions are extracted from duplicated code to ensure consistent behavior\n     8â†’/// and allow bugs to be fixed in one place.\n     9â†’\n    10â†’// MARK: - HTML Processing\n    11â†’\n    12â†’/// Strips HTML tags and entities from a string, converting it to plain text.\n    13â†’/// - Parameter html: The HTML string to strip\n    14â†’/// - Returns: Plain text with HTML tags removed and entities decoded\n    15â†’func stripHtml(_ html: String) -> String {\n    16â†’    var result = html\n    17â†’    // Remove script and style blocks\n    18â†’    result = result.replacingOccurrences(of: \"<script[^>]*>.*?</script>\", with: \"\", options: .regularExpression)\n    19â†’    result = result.replacingOccurrences(of: \"<style[^>]*>.*?</style>\", with: \"\", options: .regularExpression)\n    20â†’    // Replace common entities\n    21â†’    result = result.replacingOccurrences(of: \"&nbsp;\", with: \" \")\n    22â†’    result = result.replacingOccurrences(of: \"&amp;\", with: \"&\")\n    23â†’    result = result.replacingOccurrences(of: \"&lt;\", with: \"<\")\n    24â†’    result = result.replacingOccurrences(of: \"&gt;\", with: \">\")\n    25â†’    result = result.replacingOccurrences(of: \"&quot;\", with: \"\\\"\")\n    26â†’    // Replace <br> and </p> with newlines\n    27â†’    result = result.replacingOccurrences(of: \"<br[^>]*>\", with: \"\\n\", options: .regularExpression)\n    28â†’    result = result.replacingOccurrences(of: \"</p>\", with: \"\\n\\n\")\n    29â†’    // Remove all remaining tags\n    30â†’    result = result.replacingOccurrences(of: \"<[^>]+>\", with: \"\", options: .regularExpression)\n    31â†’    // Collapse multiple newlines\n    32â†’    result = result.replacingOccurrences(of: \"\\n{3,}\", with: \"\\n\\n\", options: .regularExpression)\n    33â†’    return result.trimmingCharacters(in: .whitespacesAndNewlines)\n    34â†’}\n    35â†’\n    36â†’// MARK: - Sender Formatting\n    37â†’\n    38â†’/// Formats a mail message sender for display.\n    39â†’/// Returns \"Name <email>\" if both are available, otherwise just the name or email.\n    40â†’/// - Parameter message: The mail message containing sender information\n    41â†’/// - Returns: Formatted sender string, or \"Unknown\" if no sender information is available\n    42â†’func formatSender(_ message: MailMessage) -> String {\n    43â†’    if let name = message.senderName, let email = message.senderEmail {\n    44â†’        return \"\\(name) <\\(email)>\"\n    45â†’    } else if let email = message.senderEmail {\n    46â†’        return email\n    47â†’    } else if let name = message.senderName {\n    48â†’        return name\n    49â†’    }\n    50â†’    return \"Unknown\"\n    51â†’}\n    52â†’\n    53â†’// MARK: - Retry Logic\n    54â†’\n    55â†’/// Configuration for retry operations with exponential backoff.\n    56â†’struct RetryConfiguration {\n    57â†’    /// Maximum number of retry attempts\n    58â†’    let maxAttempts: Int\n    59â†’\n    60â†’    /// Base delay in seconds for exponential backoff\n    61â†’    let baseDelay: TimeInterval\n    62â†’\n    63â†’    /// Maximum delay in seconds between retries\n    64â†’    let maxDelay: TimeInterval\n    65â†’\n    66â†’    /// Default configuration for mail sync operations\n    67â†’    static let `default` = RetryConfiguration(\n    68â†’        maxAttempts: 5,\n    69â†’        baseDelay: 2.0,\n    70â†’        maxDelay: 60.0\n    71â†’    )\n    72â†’}\n    73â†’\n    74â†’/// Determines if an error is transient and worth retrying.\n    75â†’/// Transient errors include database locks, timeouts, and temporary failures.\n    76â†’/// - Parameter error: The error to check\n    77â†’/// - Returns: True if the error is transient and the operation should be retried\n    78â†’func isTransientError(_ error: Error) -> Bool {\n    79â†’    let description = error.localizedDescription.lowercased()\n    80â†’    // Database lock errors, network timeouts, permission issues that may be temporary\n    81â†’    return description.contains(\"locked\") ||\n    82â†’           description.contains(\"busy\") ||\n    83â†’           description.contains(\"timeout\") ||\n    84â†’           description.contains(\"temporarily\") ||\n    85â†’           description.contains(\"try again\")\n    86â†’}\n    87â†’\n    88â†’/// Executes an operation with exponential backoff retry for transient errors.\n    89â†’/// - Parameters:\n    90â†’///   - config: The retry configuration (defaults to standard mail sync settings)\n    91â†’///   - logger: Optional logging function for retry messages\n    92â†’///   - operation: The operation to execute\n    93â†’/// - Returns: The result of the operation if successful\n    94â†’/// - Throws: The last error if all retries are exhausted or a non-transient error occurs\n    95â†’@discardableResult\n    96â†’func withRetry<T>(\n    97â†’    config: RetryConfiguration = .default,\n    98â†’    logger: ((String) -> Void)? = nil,\n    99â†’    operation: () throws -> T\n   100â†’) throws -> T {\n   101â†’    var lastError: Error?\n   102â†’    var attempt = 0\n   103â†’\n   104â†’    while attempt < config.maxAttempts {\n   105â†’        do {\n   106â†’            return try operation()\n   107â†’        } catch {\n   108â†’            lastError = error\n   109â†’            attempt += 1\n   110â†’\n   111â†’            // Check if error is transient and worth retrying\n   112â†’            if isTransientError(error) && attempt < config.maxAttempts {\n   113â†’                // Calculate exponential backoff delay with jitter\n   114â†’                let delay = min(\n   115â†’                    config.baseDelay * pow(2.0, Double(attempt - 1)),\n   116â†’                    config.maxDelay\n   117â†’                )\n   118â†’                // Add 10-20% random jitter to prevent thundering herd\n   119â†’                let jitter = delay * Double.random(in: 0.1...0.2)\n   120â†’                let totalDelay = delay + jitter\n   121â†’\n   122â†’                logger?(\"Transient error (attempt \\(attempt)/\\(config.maxAttempts)): \\(error.localizedDescription)\")\n   123â†’                logger?(\"Retrying in \\(String(format: \"%.1f\", totalDelay)) seconds...\")\n   124â†’\n   125â†’                Thread.sleep(forTimeInterval: totalDelay)\n   126â†’            } else {\n   127â†’                // Non-transient error or max retries reached\n   128â†’                break\n   129â†’            }\n   130â†’        }\n   131â†’    }\n   132â†’\n   133â†’    // All retries exhausted or non-transient error\n   134â†’    if let error = lastError {\n   135â†’        logger?(\"Operation failed after \\(attempt) attempt(s): \\(error.localizedDescription)\")\n   136â†’        throw error\n   137â†’    }\n   138â†’\n   139â†’    // This should never happen, but satisfy the compiler\n   140â†’    fatalError(\"withRetry ended without returning or throwing\")\n   141â†’}\n   142â†’\n   143â†’// MARK: - AppleScript Escaping\n   144â†’\n   145â†’/// Escapes a string for safe use in AppleScript.\n   146â†’/// Handles backslashes and double quotes.\n   147â†’/// - Parameter string: The string to escape\n   148â†’/// - Returns: The escaped string safe for AppleScript inclusion\n   149â†’func escapeAppleScript(_ string: String) -> String {\n   150â†’    string\n   151â†’        .replacingOccurrences(of: \"\\\\\", with: \"\\\\\\\\\")\n   152â†’        .replacingOccurrences(of: \"\\\"\", with: \"\\\\\\\"\")\n   153â†’}\n   154â†’\n   155â†’// MARK: - YAML Escaping\n   156â†’\n   157â†’/// Escapes a string for safe use in YAML values.\n   158â†’/// - Parameter text: The text to escape\n   159â†’/// - Returns: The escaped string safe for YAML inclusion\n   160â†’func escapeYaml(_ text: String) -> String {\n   161â†’    text.replacingOccurrences(of: \"\\\"\", with: \"\\\\\\\"\")\n   162â†’}\n   163â†’\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"7abf689b-fe15-4f6b-95ac-979b5769674c","uuid":"18301986-3b96-4b24-9c2f-491ca68190e5","tool_use_result":{"type":"text","file":{"filePath":"/Users/I852000/Library/Mobile Documents/com~apple~CloudDocs/swiftea/Sources/SwiftEACLI/Commands/MailCommandUtilities.swift","content":"import Foundation\nimport SwiftEAKit\n\n// MARK: - Mail Command Utilities\n\n/// Shared utility functions for mail commands.\n/// These functions are extracted from duplicated code to ensure consistent behavior\n/// and allow bugs to be fixed in one place.\n\n// MARK: - HTML Processing\n\n/// Strips HTML tags and entities from a string, converting it to plain text.\n/// - Parameter html: The HTML string to strip\n/// - Returns: Plain text with HTML tags removed and entities decoded\nfunc stripHtml(_ html: String) -> String {\n    var result = html\n    // Remove script and style blocks\n    result = result.replacingOccurrences(of: \"<script[^>]*>.*?</script>\", with: \"\", options: .regularExpression)\n    result = result.replacingOccurrences(of: \"<style[^>]*>.*?</style>\", with: \"\", options: .regularExpression)\n    // Replace common entities\n    result = result.replacingOccurrences(of: \"&nbsp;\", with: \" \")\n    result = result.replacingOccurrences(of: \"&amp;\", with: \"&\")\n    result = result.replacingOccurrences(of: \"&lt;\", with: \"<\")\n    result = result.replacingOccurrences(of: \"&gt;\", with: \">\")\n    result = result.replacingOccurrences(of: \"&quot;\", with: \"\\\"\")\n    // Replace <br> and </p> with newlines\n    result = result.replacingOccurrences(of: \"<br[^>]*>\", with: \"\\n\", options: .regularExpression)\n    result = result.replacingOccurrences(of: \"</p>\", with: \"\\n\\n\")\n    // Remove all remaining tags\n    result = result.replacingOccurrences(of: \"<[^>]+>\", with: \"\", options: .regularExpression)\n    // Collapse multiple newlines\n    result = result.replacingOccurrences(of: \"\\n{3,}\", with: \"\\n\\n\", options: .regularExpression)\n    return result.trimmingCharacters(in: .whitespacesAndNewlines)\n}\n\n// MARK: - Sender Formatting\n\n/// Formats a mail message sender for display.\n/// Returns \"Name <email>\" if both are available, otherwise just the name or email.\n/// - Parameter message: The mail message containing sender information\n/// - Returns: Formatted sender string, or \"Unknown\" if no sender information is available\nfunc formatSender(_ message: MailMessage) -> String {\n    if let name = message.senderName, let email = message.senderEmail {\n        return \"\\(name) <\\(email)>\"\n    } else if let email = message.senderEmail {\n        return email\n    } else if let name = message.senderName {\n        return name\n    }\n    return \"Unknown\"\n}\n\n// MARK: - Retry Logic\n\n/// Configuration for retry operations with exponential backoff.\nstruct RetryConfiguration {\n    /// Maximum number of retry attempts\n    let maxAttempts: Int\n\n    /// Base delay in seconds for exponential backoff\n    let baseDelay: TimeInterval\n\n    /// Maximum delay in seconds between retries\n    let maxDelay: TimeInterval\n\n    /// Default configuration for mail sync operations\n    static let `default` = RetryConfiguration(\n        maxAttempts: 5,\n        baseDelay: 2.0,\n        maxDelay: 60.0\n    )\n}\n\n/// Determines if an error is transient and worth retrying.\n/// Transient errors include database locks, timeouts, and temporary failures.\n/// - Parameter error: The error to check\n/// - Returns: True if the error is transient and the operation should be retried\nfunc isTransientError(_ error: Error) -> Bool {\n    let description = error.localizedDescription.lowercased()\n    // Database lock errors, network timeouts, permission issues that may be temporary\n    return description.contains(\"locked\") ||\n           description.contains(\"busy\") ||\n           description.contains(\"timeout\") ||\n           description.contains(\"temporarily\") ||\n           description.contains(\"try again\")\n}\n\n/// Executes an operation with exponential backoff retry for transient errors.\n/// - Parameters:\n///   - config: The retry configuration (defaults to standard mail sync settings)\n///   - logger: Optional logging function for retry messages\n///   - operation: The operation to execute\n/// - Returns: The result of the operation if successful\n/// - Throws: The last error if all retries are exhausted or a non-transient error occurs\n@discardableResult\nfunc withRetry<T>(\n    config: RetryConfiguration = .default,\n    logger: ((String) -> Void)? = nil,\n    operation: () throws -> T\n) throws -> T {\n    var lastError: Error?\n    var attempt = 0\n\n    while attempt < config.maxAttempts {\n        do {\n            return try operation()\n        } catch {\n            lastError = error\n            attempt += 1\n\n            // Check if error is transient and worth retrying\n            if isTransientError(error) && attempt < config.maxAttempts {\n                // Calculate exponential backoff delay with jitter\n                let delay = min(\n                    config.baseDelay * pow(2.0, Double(attempt - 1)),\n                    config.maxDelay\n                )\n                // Add 10-20% random jitter to prevent thundering herd\n                let jitter = delay * Double.random(in: 0.1...0.2)\n                let totalDelay = delay + jitter\n\n                logger?(\"Transient error (attempt \\(attempt)/\\(config.maxAttempts)): \\(error.localizedDescription)\")\n                logger?(\"Retrying in \\(String(format: \"%.1f\", totalDelay)) seconds...\")\n\n                Thread.sleep(forTimeInterval: totalDelay)\n            } else {\n                // Non-transient error or max retries reached\n                break\n            }\n        }\n    }\n\n    // All retries exhausted or non-transient error\n    if let error = lastError {\n        logger?(\"Operation failed after \\(attempt) attempt(s): \\(error.localizedDescription)\")\n        throw error\n    }\n\n    // This should never happen, but satisfy the compiler\n    fatalError(\"withRetry ended without returning or throwing\")\n}\n\n// MARK: - AppleScript Escaping\n\n/// Escapes a string for safe use in AppleScript.\n/// Handles backslashes and double quotes.\n/// - Parameter string: The string to escape\n/// - Returns: The escaped string safe for AppleScript inclusion\nfunc escapeAppleScript(_ string: String) -> String {\n    string\n        .replacingOccurrences(of: \"\\\\\", with: \"\\\\\\\\\")\n        .replacingOccurrences(of: \"\\\"\", with: \"\\\\\\\"\")\n}\n\n// MARK: - YAML Escaping\n\n/// Escapes a string for safe use in YAML values.\n/// - Parameter text: The text to escape\n/// - Returns: The escaped string safe for YAML inclusion\nfunc escapeYaml(_ text: String) -> String {\n    text.replacingOccurrences(of: \"\\\"\", with: \"\\\\\\\"\")\n}\n","numLines":163,"startLine":1,"totalLines":163}}}
