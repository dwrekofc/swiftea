{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01MNDrfCu4dNQTt2mdtRVPfC","type":"tool_result","content":"     1â†’import AppKit\n     2â†’import ArgumentParser\n     3â†’import Foundation\n     4â†’import SwiftEAKit\n     5â†’\n     6â†’// MARK: - Date Extension for ISO 8601 formatting\n     7â†’\n     8â†’extension Date {\n     9â†’    var iso8601String: String {\n    10â†’        let formatter = ISO8601DateFormatter()\n    11â†’        formatter.formatOptions = [.withInternetDateTime]\n    12â†’        return formatter.string(from: self)\n    13â†’    }\n    14â†’}\n    15â†’\n    16â†’// MARK: - Standard Error Output Helper\n    17â†’\n    18â†’/// Write a message to stderr for error/warning output.\n    19â†’/// This ensures errors dont corrupt stdout when piping (e.g., JSON output to jq).\n    20â†’private func printError(_ message: String) {\n    21â†’    fputs(\"\\(message)\\n\", stderr)\n    22â†’}\n    23â†’\n    24â†’public struct Mail: ParsableCommand {\n    25â†’    public static let configuration = CommandConfiguration(\n    26â†’        commandName: \"mail\",\n    27â†’        abstract: \"Mail operations (sync, search, show, export, actions)\",\n    28â†’        discussion: \"\"\"\n    29â†’            swea mail commands interact with Apple Mail.app to sync, search,\n    30â†’            and perform actions on your email.\n    31â†’\n    32â†’            GETTING STARTED\n    33â†’            The typical workflow is: sync -> search/show -> export\n    34â†’\n    35â†’              swea mail sync              # Sync mail to local database\n    36â†’              swea mail sync --watch      # Start automatic background sync\n    37â†’              swea mail search \"query\"    # Search synced messages\n    38â†’              swea mail export            # Export to markdown files\n    39â†’\n    40â†’            QUICK WORKFLOW\n    41â†’              # First time: sync all mail\n    42â†’              swea mail sync\n    43â†’\n    44â†’              # For ongoing use: start automatic sync\n    45â†’              swea mail sync --watch\n    46â†’\n    47â†’              # Search and export as needed\n    48â†’              swea mail search \"from:alice@example.com\"\n    49â†’              swea mail export --query \"from:alice\"\n    50â†’\n    51â†’            NOTE: 'sync' populates the database. 'export' creates .md files.\n    52â†’            Run both if you want files you can open in Obsidian.\n    53â†’\n    54â†’            AUTOMATION PERMISSION\n    55â†’            Action commands (archive, delete, move, flag, mark, reply, compose)\n    56â†’            require macOS Automation permission for Mail.app. On first run, macOS\n    57â†’            will prompt you to grant permission. To grant permission manually:\n    58â†’\n    59â†’              1. Open System Settings > Privacy & Security > Automation\n    60â†’              2. Find swea (or Terminal if running from terminal)\n    61â†’              3. Enable the toggle for Mail.app\n    62â†’\n    63â†’            SAFETY FLAGS\n    64â†’            Destructive actions (archive, delete, move) require explicit confirmation:\n    65â†’\n    66â†’              --dry-run    Preview what would happen without making changes\n    67â†’              --yes        Confirm the action and execute it\n    68â†’\n    69â†’            Running a destructive action without --yes or --dry-run will show an\n    70â†’            error explaining the requirement.\n    71â†’\n    72â†’            EXAMPLES\n    73â†’              # Sync mail from Apple Mail\n    74â†’              swea mail sync\n    75â†’\n    76â†’              # Start automatic sync (recommended for regular use)\n    77â†’              swea mail sync --watch\n    78â†’\n    79â†’              # Force a full resync if needed\n    80â†’              swea mail sync --full\n    81â†’\n    82â†’              # Search for emails from a specific sender\n    83â†’              swea mail search \"from:alice@example.com\"\n    84â†’\n    85â†’              # Export all mail to markdown files\n    86â†’              swea mail export\n    87â†’\n    88â†’              # Export filtered mail to custom location\n    89â†’              swea mail export --query \"from:alice\" --output ~/Documents/Mail\n    90â†’\n    91â†’              # Preview archiving a message (no changes made)\n    92â†’              swea mail archive --id mail-abc123 --dry-run\n    93â†’\n    94â†’              # Archive a message (requires confirmation)\n    95â†’              swea mail archive --id mail-abc123 --yes\n    96â†’\n    97â†’              # Flag a message\n    98â†’              swea mail flag --id mail-abc123 --set\n    99â†’\n   100â†’              # Compose a new email draft\n   101â†’              swea mail compose --to bob@example.com --subject \"Hello\"\n   102â†’            \"\"\",\n   103â†’        subcommands: [\n   104â†’            MailSyncCommand.self,\n   105â†’            MailSearchCommand.self,\n   106â†’            MailShowCommand.self,\n   107â†’            MailThreadCommand.self,\n   108â†’            MailThreadsCommand.self,\n   109â†’            MailExportCommand.self,\n   110â†’            MailExportThreadsCommand.self,\n   111â†’            // Action commands\n   112â†’            MailArchiveCommand.self,\n   113â†’            MailDeleteCommand.self,\n   114â†’            MailMoveCommand.self,\n   115â†’            MailFlagCommand.self,\n   116â†’            MailMarkCommand.self,\n   117â†’            MailReplyCommand.self,\n   118â†’            MailComposeCommand.self\n   119â†’        ]\n   120â†’    )\n   121â†’\n   122â†’    public init() {}\n   123â†’}\n   124â†’\n   125â†’struct MailSyncCommand: ParsableCommand {\n   126â†’    static let configuration = CommandConfiguration(\n   127â†’        commandName: \"sync\",\n   128â†’        abstract: \"Sync mail data from Apple Mail and export to Swiftea/Mail/\",\n   129â†’        discussion: \"\"\"\n   130â†’            Syncs mail data from Apple Mail to a local SQLite database for fast\n   131â†’            searching. By default, performs an incremental sync (only new/changed\n   132â†’            messages). Use --full to resync everything.\n   133â†’\n   134â†’            After sync, new messages are automatically exported as markdown files\n   135â†’            to Swiftea/Mail/ for use with Obsidian or other markdown tools.\n   136â†’            Use --no-export to disable automatic export.\n   137â†’\n   138â†’            For automatic sync, use --watch to start a background daemon that\n   139â†’            keeps your mail database and markdown exports up to date.\n   140â†’\n   141â†’            EXAMPLES\n   142â†’              swea mail sync              # Incremental sync + auto-export\n   143â†’              swea mail sync --full       # Full resync + export all\n   144â†’              swea mail sync --no-export  # Sync only, skip markdown export\n   145â†’              swea mail sync --watch      # Start automatic sync daemon\n   146â†’              swea mail sync --status     # Check sync/daemon status\n   147â†’              swea mail sync --stop       # Stop the daemon\n   148â†’            \"\"\"\n   149â†’    )\n   150â†’\n   151â†’    @Flag(name: .long, help: \"Install and start watch daemon for continuous sync\")\n   152â†’    var watch: Bool = false\n   153â†’\n   154â†’    @Flag(name: .long, help: \"Stop the watch daemon\")\n   155â†’    var stop: Bool = false\n   156â†’\n   157â†’    @Flag(name: .long, help: \"Show sync status and watch daemon state\")\n   158â†’    var status: Bool = false\n   159â†’\n   160â†’    @Flag(name: .long, help: \"Force a full sync, ignoring previous sync state\")\n   161â†’    var full: Bool = false\n   162â†’\n   163â†’    @Flag(name: .long, help: \"Show detailed progress\")\n   164â†’    var verbose: Bool = false\n   165â†’\n   166â†’    @Flag(name: .long, help: \"Run as persistent daemon with sleep/wake detection (internal use)\")\n   167â†’    var daemon: Bool = false\n   168â†’\n   169â†’    @Flag(name: .long, help: \"Disable automatic export to Swiftea/Mail/ after sync\")\n   170â†’    var noExport: Bool = false\n   171â†’\n   172â†’    @Option(name: .long, help: \"Sync interval in seconds for watch mode (default: 300, minimum: 30)\")\n   173â†’    var interval: Int?\n   174â†’\n   175â†’    // MARK: - Retry Configuration (for daemon mode)\n   176â†’\n   177â†’    /// Maximum number of retry attempts for transient errors\n   178â†’    private static let maxRetryAttempts = 5\n   179â†’\n   180â†’    /// Base delay in seconds for exponential backoff\n   181â†’    private static let baseRetryDelay: TimeInterval = 2.0\n   182â†’\n   183â†’    /// Maximum delay in seconds between retries\n   184â†’    private static let maxRetryDelay: TimeInterval = 60.0\n   185â†’\n   186â†’    /// Errors that are considered transient and worth retrying\n   187â†’    private func isTransientError(_ error: Error) -> Bool {\n   188â†’        let description = error.localizedDescription.lowercased()\n   189â†’        // Database lock errors, network timeouts, permission issues that may be temporary\n   190â†’        return description.contains(\"locked\") ||\n   191â†’               description.contains(\"busy\") ||\n   192â†’               description.contains(\"timeout\") ||\n   193â†’               description.contains(\"temporarily\") ||\n   194â†’               description.contains(\"try again\")\n   195â†’    }\n   196â†’\n   197â†’    // MARK: - Daemon-safe Logging\n   198â†’\n   199â†’    /// Detect if running as a LaunchAgent daemon (no TTY attached)\n   200â†’    private var isDaemonMode: Bool {\n   201â†’        // When run by launchd, there's no TTY and stdout is not interactive\n   202â†’        // isatty() returns non-zero if stdout is connected to a terminal, 0 otherwise\n   203â†’        return isatty(STDOUT_FILENO) == 0\n   204â†’    }\n   205â†’\n   206â†’    /// Log a message with optional timestamp prefix for daemon mode.\n   207â†’    /// Ensures output is flushed immediately so logs are captured.\n   208â†’    private func log(_ message: String) {\n   209â†’        if isDaemonMode {\n   210â†’            let timestamp = ISO8601DateFormatter().string(from: Date())\n   211â†’            fputs(\"[\\(timestamp)] \\(message)\\n\", stdout)\n   212â†’            fflush(stdout)\n   213â†’        } else {\n   214â†’            print(message)\n   215â†’        }\n   216â†’    }\n   217â†’\n   218â†’    /// Log an error message to stderr, ensuring it's flushed for daemon mode.\n   219â†’    private func logError(_ message: String) {\n   220â†’        if isDaemonMode {\n   221â†’            let timestamp = ISO8601DateFormatter().string(from: Date())\n   222â†’            fputs(\"[\\(timestamp)] ERROR: \\(message)\\n\", stderr)\n   223â†’            fflush(stderr)\n   224â†’        } else {\n   225â†’            fputs(\"Error: \\(message)\\n\", stderr)\n   226â†’        }\n   227â†’    }\n   228â†’\n   229â†’    /// Minimum allowed sync interval in seconds\n   230â†’    private static let minIntervalSeconds = 30\n   231â†’\n   232â†’    /// Default sync interval in seconds (1 minute)\n   233â†’    private static let defaultIntervalSeconds = 60\n   234â†’\n   235â†’    func validate() throws {\n   236â†’        // --watch and --stop are mutually exclusive\n   237â†’        if watch && stop {\n   238â†’            throw MailValidationError.watchAndStopMutuallyExclusive\n   239â†’        }\n   240â†’\n   241â†’        // Validate --interval if provided\n   242â†’        if let interval = interval {\n   243â†’            if interval < Self.minIntervalSeconds {\n   244â†’                throw MailValidationError.invalidInterval(minimum: Self.minIntervalSeconds)\n   245â†’            }\n   246â†’        }\n   247â†’    }\n   248â†’\n   249â†’    func run() throws {\n   250â†’        // Log startup for daemon mode debugging\n   251â†’        if isDaemonMode {\n   252â†’            log(\"mail sync started (daemon mode, pid=\\(ProcessInfo.processInfo.processIdentifier))\")\n   253â†’            log(\"working directory: \\(FileManager.default.currentDirectoryPath)\")\n   254â†’        }\n   255â†’\n   256â†’        // In daemon mode, use retry with exponential backoff for transient errors\n   257â†’        if isDaemonMode {\n   258â†’            try executeSyncWithRetry()\n   259â†’        } else {\n   260â†’            // Interactive mode: no retry, immediate failure feedback\n   261â†’            do {\n   262â†’                try executeSync()\n   263â†’            } catch {\n   264â†’                logError(\"\\(error.localizedDescription)\")\n   265â†’                throw error\n   266â†’            }\n   267â†’        }\n   268â†’    }\n   269â†’\n   270â†’    /// Execute sync with exponential backoff retry for transient errors (daemon mode)\n   271â†’    private func executeSyncWithRetry() throws {\n   272â†’        var lastError: Error?\n   273â†’        var attempt = 0\n   274â†’\n   275â†’        while attempt < Self.maxRetryAttempts {\n   276â†’            do {\n   277â†’                try executeSync()\n   278â†’                log(\"mail sync completed successfully\")\n   279â†’                return\n   280â†’            } catch {\n   281â†’                lastError = error\n   282â†’                attempt += 1\n   283â†’\n   284â†’                // Check if error is transient and worth retrying\n   285â†’                if isTransientError(error) && attempt < Self.maxRetryAttempts {\n   286â†’                    // Calculate exponential backoff delay with jitter\n   287â†’                    let delay = min(\n   288â†’                        Self.baseRetryDelay * pow(2.0, Double(attempt - 1)),\n   289â†’                        Self.maxRetryDelay\n   290â†’                    )\n   291â†’                    // Add 10-20% random jitter to prevent thundering herd\n   292â†’                    let jitter = delay * Double.random(in: 0.1...0.2)\n   293â†’                    let totalDelay = delay + jitter\n   294â†’\n   295â†’                    log(\"Transient error (attempt \\(attempt)/\\(Self.maxRetryAttempts)): \\(error.localizedDescription)\")\n   296â†’                    log(\"Retrying in \\(String(format: \"%.1f\", totalDelay)) seconds...\")\n   297â†’\n   298â†’                    Thread.sleep(forTimeInterval: totalDelay)\n   299â†’                } else {\n   300â†’                    // Non-transient error or max retries reached\n   301â†’                    break\n   302â†’                }\n   303â†’            }\n   304â†’        }\n   305â†’\n   306â†’        // All retries exhausted or non-transient error\n   307â†’        if let error = lastError {\n   308â†’            logError(\"Sync failed after \\(attempt) attempt(s): \\(error.localizedDescription)\")\n   309â†’            log(\"mail sync failed\")\n   310â†’            throw error\n   311â†’        }\n   312â†’    }\n   313â†’\n   314â†’    private func executeSync() throws {\n   315â†’        let vault = try VaultContext.require()\n   316â†’\n   317â†’        // Create mail database in vault's data folder\n   318â†’        let mailDbPath = (vault.dataFolderPath as NSString).appendingPathComponent(\"mail.db\")\n   319â†’        let mailDatabase = MailDatabase(databasePath: mailDbPath)\n   320â†’        try mailDatabase.initialize()\n   321â†’        defer { mailDatabase.close() }\n   322â†’\n   323â†’        if verbose || isDaemonMode {\n   324â†’            log(\"Mail database: \\(mailDbPath)\")\n   325â†’        }\n   326â†’\n   327â†’        // Handle --status flag\n   328â†’        if status {\n   329â†’            try showSyncStatus(mailDatabase: mailDatabase, vault: vault)\n   330â†’            return\n   331â†’        }\n   332â†’\n   333â†’        // Handle --watch flag\n   334â†’        if watch {\n   335â†’            try installWatchDaemon(vault: vault, verbose: verbose)\n   336â†’            return\n   337â†’        }\n   338â†’\n   339â†’        // Handle --stop flag\n   340â†’        if stop {\n   341â†’            try stopWatchDaemon(verbose: verbose)\n   342â†’            return\n   343â†’        }\n   344â†’\n   345â†’        // Handle --daemon flag: run as persistent daemon with sleep/wake detection\n   346â†’        if daemon {\n   347â†’            try runPersistentDaemon(mailDatabase: mailDatabase, vault: vault)\n   348â†’            return\n   349â†’        }\n   350â†’\n   351â†’        // Create sync engine\n   352â†’        let sync = MailSync(mailDatabase: mailDatabase)\n   353â†’\n   354â†’        // Wire up progress callback for verbose mode\n   355â†’        if verbose {\n   356â†’            var lastPhase: String = \"\"\n   357â†’            var lastProgressTime = Date()\n   358â†’            sync.onProgress = { progress in\n   359â†’                // Always show phase changes\n   360â†’                if progress.phase.rawValue != lastPhase {\n   361â†’                    if !lastPhase.isEmpty {\n   362â†’                        print(\"\") // newline after previous phase\n   363â†’                    }\n   364â†’                    lastPhase = progress.phase.rawValue\n   365â†’                    lastProgressTime = Date()\n   366â†’                }\n   367â†’\n   368â†’                // For message sync, show periodic updates (every 0.5s or phase change)\n   369â†’                let now = Date()\n   370â†’                let elapsed = now.timeIntervalSince(lastProgressTime)\n   371â†’                if elapsed >= 0.5 || progress.current == progress.total {\n   372â†’                    lastProgressTime = now\n   373â†’                    if progress.total > 0 {\n   374â†’                        let pct = Int(progress.percentage)\n   375â†’                        // Use carriage return to update in place\n   376â†’                        print(\"\\r  [\\(progress.phase.rawValue)] \\(progress.current)/\\(progress.total) (\\(pct)%) - \\(progress.message)\", terminator: \"\")\n   377â†’                        fflush(stdout)\n   378â†’                    } else {\n   379â†’                        print(\"\\r  [\\(progress.phase.rawValue)] \\(progress.message)\", terminator: \"\")\n   380â†’                        fflush(stdout)\n   381â†’                    }\n   382â†’                }\n   383â†’            }\n   384â†’        }\n   385â†’\n   386â†’        log(\"Syncing mail from Apple Mail...\")\n   387â†’\n   388â†’        do {\n   389â†’            let result = try sync.sync(forceFullSync: full)\n   390â†’\n   391â†’            if verbose {\n   392â†’                print(\"\") // Final newline after progress\n   393â†’            }\n   394â†’\n   395â†’            log(\"Sync complete:\")\n   396â†’            log(\"  Messages processed: \\(result.messagesProcessed)\")\n   397â†’            log(\"  Messages added: \\(result.messagesAdded)\")\n   398â†’            log(\"  Messages updated: \\(result.messagesUpdated)\")\n   399â†’            log(\"  Mailboxes: \\(result.mailboxesProcessed)\")\n   400â†’            log(\"  Duration: \\(String(format: \"%.2f\", result.duration))s\")\n   401â†’\n   402â†’            if !result.errors.isEmpty {\n   403â†’                log(\"\\nWarnings/Errors:\")\n   404â†’                for error in result.errors.prefix(10) {\n   405â†’                    log(\"  - \\(error)\")\n   406â†’                }\n   407â†’                if result.errors.count > 10 {\n   408â†’                    log(\"  ... and \\(result.errors.count - 10) more\")\n   409â†’                }\n   410â†’            }\n   411â†’\n   412â†’            // Auto-export new messages to Swiftea/Mail/ unless disabled\n   413â†’            if !noExport {\n   414â†’                try performAutoExport(mailDatabase: mailDatabase, vault: vault)\n   415â†’            }\n   416â†’\n   417â†’            // Process pending backward sync actions (archive/delete from SwiftEA to Apple Mail)\n   418â†’            try performBackwardSync(mailDatabase: mailDatabase)\n   419â†’        } catch let error as MailSyncError {\n   420â†’            logError(\"Sync failed: \\(error.localizedDescription)\")\n   421â†’            throw ExitCode.failure\n   422â†’        }\n   423â†’    }\n   424â†’\n   425â†’    // MARK: - Auto-Export\n   426â†’\n   427â†’    /// Export newly synced messages to Swiftea/Mail/ as markdown files\n   428â†’    /// Messages are grouped by conversation/thread for better organization.\n   429â†’    private func performAutoExport(mailDatabase: MailDatabase, vault: VaultContext) throws {\n   430â†’        let exportDir = (vault.dataFolderPath as NSString).appendingPathComponent(\"Mail\")\n   431â†’        let exporter = MailExporter(mailDatabase: mailDatabase)\n   432â†’\n   433â†’        do {\n   434â†’            let exportResult = try exporter.exportNewMessages(to: exportDir)\n   435â†’\n   436â†’            if exportResult.exported > 0 {\n   437â†’                log(\"Auto-export:\")\n   438â†’                log(\"  Exported \\(exportResult.exported) message(s) to Swiftea/Mail/\")\n   439â†’                if exportResult.threadsExported > 0 {\n   440â†’                    log(\"  Threads updated: \\(exportResult.threadsExported) (in threads/)\")\n   441â†’                }\n   442â†’                if exportResult.unthreadedExported > 0 {\n   443â†’                    log(\"  Unthreaded: \\(exportResult.unthreadedExported) (in unthreaded/)\")\n   444â†’                }\n   445â†’            }\n   446â†’\n   447â†’            if !exportResult.errors.isEmpty && verbose {\n   448â†’                for error in exportResult.errors.prefix(5) {\n   449â†’                    log(\"  Warning: \\(error)\")\n   450â†’                }\n   451â†’                if exportResult.errors.count > 5 {\n   452â†’                    log(\"  ... and \\(exportResult.errors.count - 5) more export errors\")\n   453â†’                }\n   454â†’            }\n   455â†’        } catch {\n   456â†’            // Log but don't fail sync if export has issues\n   457â†’            logError(\"Auto-export failed: \\(error.localizedDescription)\")\n   458â†’        }\n   459â†’    }\n   460â†’\n   461â†’    // MARK: - Backward Sync\n   462â†’\n   463â†’    /// Process pending backward sync actions (archive/delete from SwiftEA to Apple Mail)\n   464â†’    private func performBackwardSync(mailDatabase: MailDatabase) throws {\n   465â†’        let backwardSync = MailSyncBackward(mailDatabase: mailDatabase)\n   466â†’\n   467â†’        do {\n   468â†’            let result = try backwardSync.processPendingActions()\n   469â†’\n   470â†’            // Log results if any actions were processed\n   471â†’            if result.archived > 0 || result.deleted > 0 {\n   472â†’                log(\"Backward sync:\")\n   473â†’                log(\"  Archived: \\(result.archived), Deleted: \\(result.deleted)\")\n   474â†’            }\n   475â†’\n   476â†’            // Log warnings for failed actions\n   477â†’            if result.failed > 0 {\n   478â†’                logError(\"Backward sync failed for \\(result.failed) message(s)\")\n   479â†’                for error in result.errors.prefix(5) {\n   480â†’                    logError(\"  - \\(error)\")\n   481â†’                }\n   482â†’                if result.errors.count > 5 {\n   483â†’                    logError(\"  ... and \\(result.errors.count - 5) more errors\")\n   484â†’                }\n   485â†’            }\n   486â†’        } catch {\n   487â†’            // Log but don't fail sync if backward sync has issues\n   488â†’            logError(\"Backward sync failed: \\(error.localizedDescription)\")\n   489â†’        }\n   490â†’    }\n   491â†’\n   492â†’    // MARK: - Status\n   493â†’\n   494â†’    private func showSyncStatus(mailDatabase: MailDatabase, vault: VaultContext) throws {\n   495â†’        let summary = try mailDatabase.getSyncStatusSummary()\n   496â†’        let daemonStatus = getDaemonStatus()\n   497â†’\n   498â†’        print(\"Mail Sync Status\")\n   499â†’        print(\"================\")\n   500â†’        print(\"\")\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":"toolu_01UUJScLsfPA6rQ2jTmBnyLz","session_id":"bb5e378b-983f-414f-bbb3-88d76ffa4a9d","uuid":"e19b7067-ef68-493c-9c84-252a568678b5","tool_use_result":{"type":"text","file":{"filePath":"/Users/I852000/Library/Mobile Documents/com~apple~CloudDocs/swiftea/Sources/SwiftEACLI/Commands/MailCommand.swift","content":"import AppKit\nimport ArgumentParser\nimport Foundation\nimport SwiftEAKit\n\n// MARK: - Date Extension for ISO 8601 formatting\n\nextension Date {\n    var iso8601String: String {\n        let formatter = ISO8601DateFormatter()\n        formatter.formatOptions = [.withInternetDateTime]\n        return formatter.string(from: self)\n    }\n}\n\n// MARK: - Standard Error Output Helper\n\n/// Write a message to stderr for error/warning output.\n/// This ensures errors dont corrupt stdout when piping (e.g., JSON output to jq).\nprivate func printError(_ message: String) {\n    fputs(\"\\(message)\\n\", stderr)\n}\n\npublic struct Mail: ParsableCommand {\n    public static let configuration = CommandConfiguration(\n        commandName: \"mail\",\n        abstract: \"Mail operations (sync, search, show, export, actions)\",\n        discussion: \"\"\"\n            swea mail commands interact with Apple Mail.app to sync, search,\n            and perform actions on your email.\n\n            GETTING STARTED\n            The typical workflow is: sync -> search/show -> export\n\n              swea mail sync              # Sync mail to local database\n              swea mail sync --watch      # Start automatic background sync\n              swea mail search \"query\"    # Search synced messages\n              swea mail export            # Export to markdown files\n\n            QUICK WORKFLOW\n              # First time: sync all mail\n              swea mail sync\n\n              # For ongoing use: start automatic sync\n              swea mail sync --watch\n\n              # Search and export as needed\n              swea mail search \"from:alice@example.com\"\n              swea mail export --query \"from:alice\"\n\n            NOTE: 'sync' populates the database. 'export' creates .md files.\n            Run both if you want files you can open in Obsidian.\n\n            AUTOMATION PERMISSION\n            Action commands (archive, delete, move, flag, mark, reply, compose)\n            require macOS Automation permission for Mail.app. On first run, macOS\n            will prompt you to grant permission. To grant permission manually:\n\n              1. Open System Settings > Privacy & Security > Automation\n              2. Find swea (or Terminal if running from terminal)\n              3. Enable the toggle for Mail.app\n\n            SAFETY FLAGS\n            Destructive actions (archive, delete, move) require explicit confirmation:\n\n              --dry-run    Preview what would happen without making changes\n              --yes        Confirm the action and execute it\n\n            Running a destructive action without --yes or --dry-run will show an\n            error explaining the requirement.\n\n            EXAMPLES\n              # Sync mail from Apple Mail\n              swea mail sync\n\n              # Start automatic sync (recommended for regular use)\n              swea mail sync --watch\n\n              # Force a full resync if needed\n              swea mail sync --full\n\n              # Search for emails from a specific sender\n              swea mail search \"from:alice@example.com\"\n\n              # Export all mail to markdown files\n              swea mail export\n\n              # Export filtered mail to custom location\n              swea mail export --query \"from:alice\" --output ~/Documents/Mail\n\n              # Preview archiving a message (no changes made)\n              swea mail archive --id mail-abc123 --dry-run\n\n              # Archive a message (requires confirmation)\n              swea mail archive --id mail-abc123 --yes\n\n              # Flag a message\n              swea mail flag --id mail-abc123 --set\n\n              # Compose a new email draft\n              swea mail compose --to bob@example.com --subject \"Hello\"\n            \"\"\",\n        subcommands: [\n            MailSyncCommand.self,\n            MailSearchCommand.self,\n            MailShowCommand.self,\n            MailThreadCommand.self,\n            MailThreadsCommand.self,\n            MailExportCommand.self,\n            MailExportThreadsCommand.self,\n            // Action commands\n            MailArchiveCommand.self,\n            MailDeleteCommand.self,\n            MailMoveCommand.self,\n            MailFlagCommand.self,\n            MailMarkCommand.self,\n            MailReplyCommand.self,\n            MailComposeCommand.self\n        ]\n    )\n\n    public init() {}\n}\n\nstruct MailSyncCommand: ParsableCommand {\n    static let configuration = CommandConfiguration(\n        commandName: \"sync\",\n        abstract: \"Sync mail data from Apple Mail and export to Swiftea/Mail/\",\n        discussion: \"\"\"\n            Syncs mail data from Apple Mail to a local SQLite database for fast\n            searching. By default, performs an incremental sync (only new/changed\n            messages). Use --full to resync everything.\n\n            After sync, new messages are automatically exported as markdown files\n            to Swiftea/Mail/ for use with Obsidian or other markdown tools.\n            Use --no-export to disable automatic export.\n\n            For automatic sync, use --watch to start a background daemon that\n            keeps your mail database and markdown exports up to date.\n\n            EXAMPLES\n              swea mail sync              # Incremental sync + auto-export\n              swea mail sync --full       # Full resync + export all\n              swea mail sync --no-export  # Sync only, skip markdown export\n              swea mail sync --watch      # Start automatic sync daemon\n              swea mail sync --status     # Check sync/daemon status\n              swea mail sync --stop       # Stop the daemon\n            \"\"\"\n    )\n\n    @Flag(name: .long, help: \"Install and start watch daemon for continuous sync\")\n    var watch: Bool = false\n\n    @Flag(name: .long, help: \"Stop the watch daemon\")\n    var stop: Bool = false\n\n    @Flag(name: .long, help: \"Show sync status and watch daemon state\")\n    var status: Bool = false\n\n    @Flag(name: .long, help: \"Force a full sync, ignoring previous sync state\")\n    var full: Bool = false\n\n    @Flag(name: .long, help: \"Show detailed progress\")\n    var verbose: Bool = false\n\n    @Flag(name: .long, help: \"Run as persistent daemon with sleep/wake detection (internal use)\")\n    var daemon: Bool = false\n\n    @Flag(name: .long, help: \"Disable automatic export to Swiftea/Mail/ after sync\")\n    var noExport: Bool = false\n\n    @Option(name: .long, help: \"Sync interval in seconds for watch mode (default: 300, minimum: 30)\")\n    var interval: Int?\n\n    // MARK: - Retry Configuration (for daemon mode)\n\n    /// Maximum number of retry attempts for transient errors\n    private static let maxRetryAttempts = 5\n\n    /// Base delay in seconds for exponential backoff\n    private static let baseRetryDelay: TimeInterval = 2.0\n\n    /// Maximum delay in seconds between retries\n    private static let maxRetryDelay: TimeInterval = 60.0\n\n    /// Errors that are considered transient and worth retrying\n    private func isTransientError(_ error: Error) -> Bool {\n        let description = error.localizedDescription.lowercased()\n        // Database lock errors, network timeouts, permission issues that may be temporary\n        return description.contains(\"locked\") ||\n               description.contains(\"busy\") ||\n               description.contains(\"timeout\") ||\n               description.contains(\"temporarily\") ||\n               description.contains(\"try again\")\n    }\n\n    // MARK: - Daemon-safe Logging\n\n    /// Detect if running as a LaunchAgent daemon (no TTY attached)\n    private var isDaemonMode: Bool {\n        // When run by launchd, there's no TTY and stdout is not interactive\n        // isatty() returns non-zero if stdout is connected to a terminal, 0 otherwise\n        return isatty(STDOUT_FILENO) == 0\n    }\n\n    /// Log a message with optional timestamp prefix for daemon mode.\n    /// Ensures output is flushed immediately so logs are captured.\n    private func log(_ message: String) {\n        if isDaemonMode {\n            let timestamp = ISO8601DateFormatter().string(from: Date())\n            fputs(\"[\\(timestamp)] \\(message)\\n\", stdout)\n            fflush(stdout)\n        } else {\n            print(message)\n        }\n    }\n\n    /// Log an error message to stderr, ensuring it's flushed for daemon mode.\n    private func logError(_ message: String) {\n        if isDaemonMode {\n            let timestamp = ISO8601DateFormatter().string(from: Date())\n            fputs(\"[\\(timestamp)] ERROR: \\(message)\\n\", stderr)\n            fflush(stderr)\n        } else {\n            fputs(\"Error: \\(message)\\n\", stderr)\n        }\n    }\n\n    /// Minimum allowed sync interval in seconds\n    private static let minIntervalSeconds = 30\n\n    /// Default sync interval in seconds (1 minute)\n    private static let defaultIntervalSeconds = 60\n\n    func validate() throws {\n        // --watch and --stop are mutually exclusive\n        if watch && stop {\n            throw MailValidationError.watchAndStopMutuallyExclusive\n        }\n\n        // Validate --interval if provided\n        if let interval = interval {\n            if interval < Self.minIntervalSeconds {\n                throw MailValidationError.invalidInterval(minimum: Self.minIntervalSeconds)\n            }\n        }\n    }\n\n    func run() throws {\n        // Log startup for daemon mode debugging\n        if isDaemonMode {\n            log(\"mail sync started (daemon mode, pid=\\(ProcessInfo.processInfo.processIdentifier))\")\n            log(\"working directory: \\(FileManager.default.currentDirectoryPath)\")\n        }\n\n        // In daemon mode, use retry with exponential backoff for transient errors\n        if isDaemonMode {\n            try executeSyncWithRetry()\n        } else {\n            // Interactive mode: no retry, immediate failure feedback\n            do {\n                try executeSync()\n            } catch {\n                logError(\"\\(error.localizedDescription)\")\n                throw error\n            }\n        }\n    }\n\n    /// Execute sync with exponential backoff retry for transient errors (daemon mode)\n    private func executeSyncWithRetry() throws {\n        var lastError: Error?\n        var attempt = 0\n\n        while attempt < Self.maxRetryAttempts {\n            do {\n                try executeSync()\n                log(\"mail sync completed successfully\")\n                return\n            } catch {\n                lastError = error\n                attempt += 1\n\n                // Check if error is transient and worth retrying\n                if isTransientError(error) && attempt < Self.maxRetryAttempts {\n                    // Calculate exponential backoff delay with jitter\n                    let delay = min(\n                        Self.baseRetryDelay * pow(2.0, Double(attempt - 1)),\n                        Self.maxRetryDelay\n                    )\n                    // Add 10-20% random jitter to prevent thundering herd\n                    let jitter = delay * Double.random(in: 0.1...0.2)\n                    let totalDelay = delay + jitter\n\n                    log(\"Transient error (attempt \\(attempt)/\\(Self.maxRetryAttempts)): \\(error.localizedDescription)\")\n                    log(\"Retrying in \\(String(format: \"%.1f\", totalDelay)) seconds...\")\n\n                    Thread.sleep(forTimeInterval: totalDelay)\n                } else {\n                    // Non-transient error or max retries reached\n                    break\n                }\n            }\n        }\n\n        // All retries exhausted or non-transient error\n        if let error = lastError {\n            logError(\"Sync failed after \\(attempt) attempt(s): \\(error.localizedDescription)\")\n            log(\"mail sync failed\")\n            throw error\n        }\n    }\n\n    private func executeSync() throws {\n        let vault = try VaultContext.require()\n\n        // Create mail database in vault's data folder\n        let mailDbPath = (vault.dataFolderPath as NSString).appendingPathComponent(\"mail.db\")\n        let mailDatabase = MailDatabase(databasePath: mailDbPath)\n        try mailDatabase.initialize()\n        defer { mailDatabase.close() }\n\n        if verbose || isDaemonMode {\n            log(\"Mail database: \\(mailDbPath)\")\n        }\n\n        // Handle --status flag\n        if status {\n            try showSyncStatus(mailDatabase: mailDatabase, vault: vault)\n            return\n        }\n\n        // Handle --watch flag\n        if watch {\n            try installWatchDaemon(vault: vault, verbose: verbose)\n            return\n        }\n\n        // Handle --stop flag\n        if stop {\n            try stopWatchDaemon(verbose: verbose)\n            return\n        }\n\n        // Handle --daemon flag: run as persistent daemon with sleep/wake detection\n        if daemon {\n            try runPersistentDaemon(mailDatabase: mailDatabase, vault: vault)\n            return\n        }\n\n        // Create sync engine\n        let sync = MailSync(mailDatabase: mailDatabase)\n\n        // Wire up progress callback for verbose mode\n        if verbose {\n            var lastPhase: String = \"\"\n            var lastProgressTime = Date()\n            sync.onProgress = { progress in\n                // Always show phase changes\n                if progress.phase.rawValue != lastPhase {\n                    if !lastPhase.isEmpty {\n                        print(\"\") // newline after previous phase\n                    }\n                    lastPhase = progress.phase.rawValue\n                    lastProgressTime = Date()\n                }\n\n                // For message sync, show periodic updates (every 0.5s or phase change)\n                let now = Date()\n                let elapsed = now.timeIntervalSince(lastProgressTime)\n                if elapsed >= 0.5 || progress.current == progress.total {\n                    lastProgressTime = now\n                    if progress.total > 0 {\n                        let pct = Int(progress.percentage)\n                        // Use carriage return to update in place\n                        print(\"\\r  [\\(progress.phase.rawValue)] \\(progress.current)/\\(progress.total) (\\(pct)%) - \\(progress.message)\", terminator: \"\")\n                        fflush(stdout)\n                    } else {\n                        print(\"\\r  [\\(progress.phase.rawValue)] \\(progress.message)\", terminator: \"\")\n                        fflush(stdout)\n                    }\n                }\n            }\n        }\n\n        log(\"Syncing mail from Apple Mail...\")\n\n        do {\n            let result = try sync.sync(forceFullSync: full)\n\n            if verbose {\n                print(\"\") // Final newline after progress\n            }\n\n            log(\"Sync complete:\")\n            log(\"  Messages processed: \\(result.messagesProcessed)\")\n            log(\"  Messages added: \\(result.messagesAdded)\")\n            log(\"  Messages updated: \\(result.messagesUpdated)\")\n            log(\"  Mailboxes: \\(result.mailboxesProcessed)\")\n            log(\"  Duration: \\(String(format: \"%.2f\", result.duration))s\")\n\n            if !result.errors.isEmpty {\n                log(\"\\nWarnings/Errors:\")\n                for error in result.errors.prefix(10) {\n                    log(\"  - \\(error)\")\n                }\n                if result.errors.count > 10 {\n                    log(\"  ... and \\(result.errors.count - 10) more\")\n                }\n            }\n\n            // Auto-export new messages to Swiftea/Mail/ unless disabled\n            if !noExport {\n                try performAutoExport(mailDatabase: mailDatabase, vault: vault)\n            }\n\n            // Process pending backward sync actions (archive/delete from SwiftEA to Apple Mail)\n            try performBackwardSync(mailDatabase: mailDatabase)\n        } catch let error as MailSyncError {\n            logError(\"Sync failed: \\(error.localizedDescription)\")\n            throw ExitCode.failure\n        }\n    }\n\n    // MARK: - Auto-Export\n\n    /// Export newly synced messages to Swiftea/Mail/ as markdown files\n    /// Messages are grouped by conversation/thread for better organization.\n    private func performAutoExport(mailDatabase: MailDatabase, vault: VaultContext) throws {\n        let exportDir = (vault.dataFolderPath as NSString).appendingPathComponent(\"Mail\")\n        let exporter = MailExporter(mailDatabase: mailDatabase)\n\n        do {\n            let exportResult = try exporter.exportNewMessages(to: exportDir)\n\n            if exportResult.exported > 0 {\n                log(\"Auto-export:\")\n                log(\"  Exported \\(exportResult.exported) message(s) to Swiftea/Mail/\")\n                if exportResult.threadsExported > 0 {\n                    log(\"  Threads updated: \\(exportResult.threadsExported) (in threads/)\")\n                }\n                if exportResult.unthreadedExported > 0 {\n                    log(\"  Unthreaded: \\(exportResult.unthreadedExported) (in unthreaded/)\")\n                }\n            }\n\n            if !exportResult.errors.isEmpty && verbose {\n                for error in exportResult.errors.prefix(5) {\n                    log(\"  Warning: \\(error)\")\n                }\n                if exportResult.errors.count > 5 {\n                    log(\"  ... and \\(exportResult.errors.count - 5) more export errors\")\n                }\n            }\n        } catch {\n            // Log but don't fail sync if export has issues\n            logError(\"Auto-export failed: \\(error.localizedDescription)\")\n        }\n    }\n\n    // MARK: - Backward Sync\n\n    /// Process pending backward sync actions (archive/delete from SwiftEA to Apple Mail)\n    private func performBackwardSync(mailDatabase: MailDatabase) throws {\n        let backwardSync = MailSyncBackward(mailDatabase: mailDatabase)\n\n        do {\n            let result = try backwardSync.processPendingActions()\n\n            // Log results if any actions were processed\n            if result.archived > 0 || result.deleted > 0 {\n                log(\"Backward sync:\")\n                log(\"  Archived: \\(result.archived), Deleted: \\(result.deleted)\")\n            }\n\n            // Log warnings for failed actions\n            if result.failed > 0 {\n                logError(\"Backward sync failed for \\(result.failed) message(s)\")\n                for error in result.errors.prefix(5) {\n                    logError(\"  - \\(error)\")\n                }\n                if result.errors.count > 5 {\n                    logError(\"  ... and \\(result.errors.count - 5) more errors\")\n                }\n            }\n        } catch {\n            // Log but don't fail sync if backward sync has issues\n            logError(\"Backward sync failed: \\(error.localizedDescription)\")\n        }\n    }\n\n    // MARK: - Status\n\n    private func showSyncStatus(mailDatabase: MailDatabase, vault: VaultContext) throws {\n        let summary = try mailDatabase.getSyncStatusSummary()\n        let daemonStatus = getDaemonStatus()\n\n        print(\"Mail Sync Status\")\n        print(\"================\")\n        print(\"\")","numLines":500,"startLine":1,"totalLines":3077}}}
